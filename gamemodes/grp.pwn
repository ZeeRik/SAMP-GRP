		    return SendClientMessage(playerid, COLOR_GREY, !"У администратора нет выговоров");

        if(!AdminInfo[ids][aLevel])
		    return SendClientMessage(playerid, COLOR_GREY, !"Игрок не является администратором");

	    if(AdminInfo[ids][aLevel] >= AdminInfo[playerid][aLevel] && PlayerInfo[playerid][pID] != 1 && PlayerInfo[playerid][pID] != 2)
		    return SendClientMessage(playerid, COLOR_WHITE, !"У вас нет прав снять выговор данному администратору");

        AdminInfo[ids][aVigs] --;

        format(stringer1, sizeof(stringer1), "Главный администратор %s снял администратору %s выговор (%d/3). Причина: %s", PlayerInfo[playerid][pNames], PlayerInfo[ids][pNames], AdminInfo[ids][aVigs], reason);
