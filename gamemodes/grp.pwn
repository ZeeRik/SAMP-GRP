public OnPlayerClickMap(playerid, Float:fX, Float:fY, Float:fZ)
{
	new bizWarType = GetPVarInt(playerid, "biz_war_map");
    if(bizWarType > 0)
	{
        DeletePVar(playerid, "biz_war_map");
        
       	if(!ISAMAFIA(playerid))
	    	return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Вы не член мафии");

   	 	else if(PlayerInfo[playerid][pRank] < 6)
	    	return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Ваш ранг недостаточен");

    	new h; gettime(h);

		if(h < 9 || h > 23)
			return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Использовать команду можно только с 09:00 до 00:00");

		if(BizzWars != 0)
			return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}На данный момент уже идет захват бизнеса!");

		else if(FrakCD[PlayerInfo[playerid][pMember]] > 0)
			return SendMes(playerid, COLOR_GREY, "Война за бизнес будет доступна через %s.", Convert(FrakCD[PlayerInfo[playerid][pMember]]));

    	else if(gFractionFreeze[PlayerInfo[playerid][pMember]-1] > gettime())
			return SendClientMessage(playerid, COLOR_GREY, "Ваша мафия заморожена.");
		
    	if( fX < -2860 || fX > 881 || fY > 2860 || fY < 746 || ( fY < 2139 && fX < -1852 ) || ( fY < 1776 && fX < -1712 ) || ( fY < 1654 && fX < -1240 ) || ( fY < 1077 && fX < -960 ))
		{
			SendClientMessage(playerid, COLOR_GREY, "Невозможно устроить стрелу на данной территории!" ) ;
			return SendClientMessage(playerid, COLOR_GREY, "Это должна быть территория в пригороде г.Лас-Вентурас!" ) ;
		}

		for(new i = 1; i <= TOTALBIZZ; i++)
		{
	    	if(!IsPlayerInRangeOfPoint(playerid, 15.0, BizzInfo[i][bEntrx], BizzInfo[i][bEntry], BizzInfo[i][bEntrz]))
				continue;

			if(BizzInfo[i][bMafia] == PlayerInfo[playerid][pMember])
				return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Этот бизнес принадлежит Вашей мафии");

        	if(BizzInfo[i][bMafia] == 0)
			{
				BizzInfo[i][bMafia] = PlayerInfo[playerid][pMember];
				FrakCD[PlayerInfo[playerid][pMember]] = 3600;
				SaveBizz(i);

				format(stringer1, sizeof(stringer1), "[F] Был взят под контроль бизнес \"%s\"", BizzInfo[i][bName]);
				SendFamilyMessage(PlayerInfo[playerid][pMember], 0x4682B4FF, stringer1);

				return SendClientMessage(playerid, COLOR_WHITE, "У данного бизнеса не было крыши. Данный бизнес переходит в руки вашей мафии.");
			}

			if(gFractionFreeze[BizzInfo[i][bMafia]-1] > gettime())
				return SendClientMessage(playerid, COLOR_GREY, "Мафия, владеющая данным бизнесом заморожена. Ожидайте сообщение в чате о снятие заморозки.");

        	if(IsPlayerInBandOnline(PlayerInfo[playerid][pMember]) < 2)
				return SendClientMessage(playerid,COLOR_GREY, "В вашей мафии мало игроков онлайн!");

			if(IsPlayerInBandOnline(BizzInfo[i][bMafia]) < 2)
				return SendMes(playerid, COLOR_GREY, "У мафии \"%s\" мало игроков онлайн!", GetPlayerFracName[BizzInfo[i][bMafia]]);

			get_gz_pos( fX, fY, 80.0, gBizzWarZone[0], gBizzWarZone[1], gBizzWarZone[2], gBizzWarZone[3]);

	    	BizzWarsNumberZone = GangZoneCreate(gBizzWarZone[0], gBizzWarZone[1], gBizzWarZone[2], gBizzWarZone[3]);
	    	GangZoneShowForAll(BizzWarsNumberZone, GetGangZoneColor(BizzInfo[i][bMafia]));
	    	GangZoneFlashForAll(BizzWarsNumberZone, GetGangZoneColor(PlayerInfo[playerid][pMember]));

			BizzWar[bwNapad] = PlayerInfo[playerid][pMember];
			BizzWar[bwGuard] = BizzInfo[i][bMafia];
			BizzWarsNumber = BizzInfo[i][bID];
			FrakCD[PlayerInfo[playerid][pMember]] = 3600;
			BizzWarTime = 1200;
			BizzWars = bizWarType;
			GangInfo[PlayerInfo[playerid][pMember]][capture] = 1;
			GangInfo[BizzInfo[i][bMafia]][capture] = 1;
			GangInfo[BizzInfo[i][bMafia]][captureid] = PlayerInfo[playerid][pMember];
			GangInfo[PlayerInfo[playerid][pMember]][captureid] = PlayerInfo[playerid][pMember];
			GangInfo[PlayerInfo[playerid][pMember]][gangnumber] = 1;
			GangInfo[BizzInfo[i][bMafia]][gangnumber] = 0;
			
			format(stringer1, sizeof(stringer1),"%s: %d, KILLS: %d~n~%s: %d, KILLS: %d~n~~n~TIME: %s", GetGangNameTD(BizzWar[bwNapad]), BizzWar[bwCostNapad], GangInfo[BizzWar[bwNapad]][score], GetGangNameTD(BizzWar[bwGuard]), BizzWar[bwCostGuard], GangInfo[BizzWar[bwGuard]][score], Convert(BizzWarTime));
            TextDrawSetString(TextBizzWar, stringer1);
            
            format(stringer1, sizeof(stringer1), "[Внимание]: %s забил стрелу %s за бизнес %s. Место встречи отмечено на карте.",PlayerInfo[playerid][pNames], GetPlayerFracName[BizzInfo[i][bMafia]],BizzInfo[i][bName]);
			foreach (new x : Player)
			{
		    	if(!gPlayerLogged{x})
					continue;
					
				if(BizzWar[bwNapad] != PlayerInfo[x][pMember] && BizzWar[bwGuard] != PlayerInfo[x][pMember])
					continue;
					
				TextDrawShowForPlayer(x, TextBizzWar);
				SendClientMessage(x, 0xFF2400FF, stringer1);
				if(bizWarType == 1)
				{
				    SendClientMessage(x, 0xFF2400FF, !"[Внимание] Война будет происходить по убийствам");
					SendClientMessage(x, 0xFF2400FF, !"[Внимание] Чтобы выиграть войну, Ваша мафия должна убить больше врагов в квадрате!");
				}
				else if(bizWarType == 2)
				{
				    SendClientMessage(x, 0xFF2400FF, !"[Внимание] Война будет происходить по кол-ву человек в зоне");
					SendClientMessage(x, 0xFF2400FF, !"[Внимание] Чтобы выиграть войну, Ваша мафия должна выбить врагов из квадрата!");
				}
			}
			return 1;
		}
		return SendClientMessage(playerid, COLOR_GREY,"Вы не у бизнеса!");
    }
    else if(AdminInfo[playerid][aLevel] >= 1 && AdminInfo[playerid][aLogin])
	{
		if(GetPlayerInterior(playerid) != 0) SetPlayerInterior(playerid, 0);
		if(GetPlayerVirtualWorld(playerid) != 0) SetPlayerVirtualWorldEx(playerid, 0);
		SetPlayerPosFindZ( playerid, fX, fY, 999.0 );
		SendClientMessage(playerid,COLOR_GREY,"Инфо: Вы успешно телепортировались");
	}
	return true;
}

stock get_gz_pos ( Float:x, Float:y, Float:radius, &Float:minx, &Float:miny, &Float:maxx, &Float:maxy )
{
	minx = x - radius;
	miny = y - radius;
	maxx = x + radius;
	maxy = y + radius;
	return 1;
}
