publics: CreateFamily(playerid, familyName[], type)
{
    if(cache_num_rows())
	{
        SendMes(playerid, -1, "Семья {FF4500}%s{FFFFFF} уже существует", familyName);

		if(type == 1)
		    ShowPlayerDialog(playerid, 26371, DIALOG_STYLE_INPUT, !"{FFEFD5}Создание семьи", !"{6495ED}Введите название своей семьи!", !"Создать", !"Отмена");
		else
		    ShowPlayerDialog(playerid, 26379, DIALOG_STYLE_INPUT, !"{FFEFD5}Имя семьи", !"{FFFFFF}Введите новое имя семьи", !"Далее", !"Отмена");

		return 1;
    }

    if(type == 1)
    {
        fText[playerid] = CreateDynamic3DTextLabel(familyName, -1, 0, 0, 0.3, 25, playerid);
        strmid(FamilyInfo[playerid][fLeader], PlayerInfo[playerid][pNames], 0, strlen(PlayerInfo[playerid][pNames]), 24);
        strmid(FamilyInfo[playerid][fName], familyName, 0, strlen(familyName), 32);
        FamilyInfo[playerid][fDate] = gettime();

        mysql_format(connects, mysql_query_, sizeof(mysql_query_), "INSERT INTO family (name, leader, `date`) VALUES ('%e', '%e', %d)", familyName, FamilyInfo[playerid][fLeader], FamilyInfo[playerid][fDate]);
	    mysql_tquery(connects, mysql_query_, "GetFamilyID", "d", playerid);

	    PlayerInfo[playerid][pDonatemoney2] -= 3000;
	    return SendMes(playerid, -1, "Поздравляем! Вы создали семью {2E8B57}%s{FFFFFF}. Чтобы открыть меню семьи введите /fmenu.", familyName);
	}

	foreach(new i : Player) {
		if(PlayerInfo[i][pFamily] != PlayerInfo[playerid][pFamily])
			continue;

		strmid(FamilyInfo[i][fName], familyName, 0, strlen(familyName), 32);

	}

	mysql_format(connects, mysql_query_, sizeof(mysql_query_), "UPDATE family SET name = '%e' WHERE id = %d", FamilyInfo[playerid][fName], PlayerInfo[playerid][pFamily]);
	return mysql_tquery(connects, mysql_query_, "", "" ) ;
}
