publics: OnCheckPlayerOnline(playerid, name[])
{
  	new
		rows = cache_num_rows();

	if(!rows)
		return SendClientMessage(playerid, COLOR_GREY, "Игрок не найден!");

    new online_date[20],
		online_sec = 0,
		afk_sec = 0,
		total_game = 0,
		total_afk = 0;

    if(rows == 1)
		format(stringer, sizeof(stringer), "{3399FF}Онлайн за последний день\n\n");
    else
		format(stringer, sizeof(stringer), "{3399FF}Онлайн за последние %d %s\n\n", rows, PluralToText(rows, "день", "дня", "дней"));

	for(new i; i < rows; i++)
	{
		cache_get_value_name(i, "date", online_date, 20);
		cache_get_value_name_int(i, "online_sec", online_sec);
		cache_get_value_name_int(i, "afk_sec", afk_sec);

		total_game += online_sec;
		total_afk += afk_sec;

		format(stringer, sizeof(stringer), "%s{FFFFFF}%s\t\t%s {FF9900}[AFK: %s]\n", stringer, online_date, Convert(online_sec), Convert(afk_sec));
	}
	format(stringer, sizeof(stringer), "%s\n\t{FFFFFF}Общий онлайн: %s {FF9900}[AFK: %s]",stringer, Convert(total_game), Convert(total_afk));

    return ShowPlayerDialog(playerid, 1003, DIALOG_STYLE_MSGBOX, name, stringer, "Назад", "Закрыть");
}
