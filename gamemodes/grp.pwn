publics: Logs(playerid, type, page)
{
	new rows;
	cache_get_row_count(rows);
	if(!rows) return SendClientMessage(playerid, COLOR_GREY, "Истории не найдено.");
	enum LOG_DATE
	{
		number,
		act,
		summa,
		trasfer_number,
		nick[24],
		logdate[32]
	}
	new Crazy[1][LOG_DATE], string[1500];
	if(type == 1) {
		for(new i; i < rows; i++)
    	{
    		cache_get_value_name_int(i, "number", Crazy[0][number]);
    		cache_get_value_name_int(i, "act", Crazy[0][act]);
    		cache_get_value_name_int(i, "summa", Crazy[0][summa]);
    		cache_get_value_name_int(i, "trasfer_number", Crazy[0][trasfer_number]);
        	cache_get_value_name(i, "nick", Crazy[0][nick], 24);
        	cache_get_value_name(i, "date", Crazy[0][logdate], 32);
        	if(Crazy[0][act] == 0) format(stringer1, sizeof(stringer1), "{FF0000}[%s]{FFFFFF} На счет зачисленно %s вирт\n", Crazy[0][logdate],formatInt(Crazy[0][summa]));
        	else if(Crazy[0][act] == 1) format(stringer1, sizeof(stringer1), "{FF0000}[%s]{FFFFFF} Со счета снято %s вирт\n", Crazy[0][logdate],formatInt(Crazy[0][summa]));
        	else if(Crazy[0][act] == 2 && Crazy[0][number] == PlayerInfo[playerid][pID]) format(stringer1, sizeof(stringer1), "{FF0000}[%s]{FFFFFF} На основной счет (им. %s) переведено %s вирт\n", Crazy[0][logdate],Crazy[0][nick],formatInt(Crazy[0][summa]));
        	else if(Crazy[0][act] == 2) format(stringer1, sizeof(stringer1), "{FF0000}[%s]{FFFFFF} На счет %d (им. %s) переведено %s вирт\n", Crazy[0][logdate],Crazy[0][trasfer_number],Crazy[0][nick],formatInt(Crazy[0][summa]));
        	strcat(string, stringer1);
    	}
    	ShowPlayerDialog(playerid, 0000, DIALOG_STYLE_MSGBOX, "{FF6F00}История операций", string, "Принять", "");
	}
	else if(type == 2) {
		for(new i; i < rows; i++)
    	{
        	cache_get_value_name(i, "ip", Crazy[0][nick], 32);
        	cache_get_value_name(i, "date", Crazy[0][logdate], 32);
        	cache_get_value_name_int(i, "status", Crazy[0][act]);
  			format(stringer1, sizeof(stringer1), "{FFFFFF}%d. Дата: %s | IP: %s - %s\n", i+1, Crazy[0][logdate], Crazy[0][nick], Crazy[0][act] ? ("{09B000}Удачно{FFFFFF}") : ("{FF0000}Неудачно{FFFFFF}"));
        	strcat(string, stringer1);
		}
    	ShowPlayerDialog(playerid, 0000, DIALOG_STYLE_MSGBOX, "{FF6F00}Информация о вводах каптчи", string, "Принять", "");
	}
	else if(type == 3) {
	    format(stringer1, sizeof(stringer1),"{FFFFFF}#\t{FFFFFF}Ник\t{FFFFFF}Последний вход\t{FFFFFF}Статус\n{FF6F00}>>\n<<\n"), strcat(string,stringer1);
		for(new i; i < rows; i++)
    	{
        	cache_get_value_name(i, "name", Crazy[0][nick], 32);
        	cache_get_value_name(i, "geton_date", Crazy[0][logdate], 32);
        	cache_get_value_name_int(i, "online", Crazy[0][act]);
        	format(stringer1,sizeof(stringer1), "%d.\t%s\t{FF6F00}%s{FFFFFF}\t{FF6F00}%s{FFFFFF}\n",i+1,Crazy[0][nick],Crazy[0][logdate],Crazy[0][act] != 1001 ? ("{09B000}Online{FFFFFF}") : ("{FF0000}OFFline{FFFFFF}")), strcat(string,stringer1);
		}
		SetPVarInt(playerid, #page_str, page);
		format(stringer1, sizeof(stringer1), "{FF6F00}Игроки с одинаковыми IP (страница №%d)", page);
    	return ShowPlayerDialog(playerid, 793, DIALOG_STYLE_TABLIST_HEADERS, stringer1, string, "Принять", "Отмена");
	}
	else if(type == 4) {
	    format(stringer1, sizeof(stringer1),"{FFFFFF}Ник\t{FFFFFF}Уровень\t{FFFFFF}Ответы\t{FFFFFF}Статус\n"), strcat(string,stringer1);
	    foreach (new i : Player)
	    {
		    if(PlayerInfo[i][pHelper] == 0) continue;
		    format(stringer1,sizeof(stringer1), "%s\t%d\t%d\t{09B000}Online\n",PlayerInfo[i][pNames],PlayerInfo[i][pHelper],PlayerInfo[i][pHotvet]), strcat(string,stringer1);
		}
		for(new i; i < rows; i++)
    	{
        	cache_get_value_name(i, "name", Crazy[0][nick], 32);
        	cache_get_value_name_int(i, "helper", Crazy[0][summa]);
        	cache_get_value_name_int(i, "helper_asks", Crazy[0][number]);
        	format(stringer1,sizeof(stringer1), "%s\t%d\t%d\t{FF0000}OFFline\n",Crazy[0][nick],Crazy[0][summa],Crazy[0][number]), strcat(string,stringer1);
		}
    	ShowPlayerDialog(playerid, 0000, DIALOG_STYLE_TABLIST_HEADERS, "{FF6F00}Статистика хелперов", string, "Принять", "Отмена");
	}
	else if(type == 5) {
		new time[11];
	    format(stringer1, sizeof(stringer1),"{FFFFFF}#\t{FFFFFF}С\t{FFFFFF}На\t{FFFFFF}Время\n"), strcat(string,stringer1);
		for(new i; i < rows; i++)
    	{
        	cache_get_value_name(i, "old_nick", Crazy[0][nick], 24);
        	cache_get_value_name(i, "new_nick", Crazy[0][logdate], 24);
        	cache_get_value_name(i, "date", time, 11);
        	format(stringer1,sizeof(stringer1), "%d.\t%s\t{FF6F00}%s{FFFFFF}\t%s{FFFFFF}\n",i+1,Crazy[0][nick],Crazy[0][logdate], time), strcat(string,stringer1);
		}
    	return ShowPlayerDialog(playerid, 0000, DIALOG_STYLE_TABLIST_HEADERS, "{FF6F00}История измененных ников", string, "Принять", "Отмена");
	}
	return true;
}
