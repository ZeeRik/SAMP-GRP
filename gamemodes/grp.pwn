publics: Givedonate(playerid, tmp[], donate)
{
	new rows, money;
	cache_get_row_count(rows);
	if(!rows) return SendMes(playerid, COLOR_GREY, "Аккаунт %s не найден в базе данных.",tmp);
	if(donate > 5000 || donate < 0) return SendClientMessage(playerid,COLOR_WHITE,"Ошибка: Нельзя меньше 0 и больше 5000");

	new ids = GetPlayerID(tmp);
	if(IsPlayerConnected(ids)) {
	    PlayerInfo[ids][pDonatemoney2] += donate;
        SendMes(ids, COLOR_WHITE,"Администратор: %s выдал вам %d донат-очков, теперь у вас %d донат очков.", PlayerInfo[playerid][pNames], donate, PlayerInfo[ids][pDonatemoney2]);
    }

    cache_get_value_name_int(0, "donate", money);
	mysql_format(connects, mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `donate` = '%d' WHERE `name` = '%e'", money+donate, tmp);
