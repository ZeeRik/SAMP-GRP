publics: CheckMail(playerid, email[])
{
	if(cache_num_rows() != 0)
		return ShowPlayerDialog(playerid, 3, DIALOG_STYLE_MSGBOX, !COLOR_TITLE"Ошибка", !COLOR_ERROR"Указанный электронный адрес занят другим аккаунтом! Укажите другой электронный адрес!", !"Изменить", !"Назад");
		
    foreach (new i : Player) {
    	if(gPlayerLogged{i}) continue;
    	if(strcmp(PlayerInfo[i][pEmail], email) == 0 && !isnull(PlayerInfo[i][pEmail])) {
    	    return ShowPlayerDialog(playerid, 3, DIALOG_STYLE_MSGBOX, !COLOR_TITLE"Ошибка", !COLOR_ERROR"Указанный электронный адрес занят другим аккаунтом! Укажите другой электронный адрес!", !"Изменить", !"Назад");
    	}
	}

	strmid(PlayerInfo[playerid][pEmail], email, 0, strlen(email), 65);
	return ShowPlayerDialog(playerid, 4, DIALOG_STYLE_LIST, !COLOR_TITLE"Выберите национальность", !COLOR_TEXT"Американец\nИтальянец\nНемец\nИспанец\nФранцуз\nРусский\nКитаец\nЯпонец", !"Выбрать", !"Назад");
}
