CMD:veh(playerid, params[])
{
	if(AdminInfo[playerid][aLevel] < 5 || !AdminInfo[playerid][aLogin])
		return 1;
		
	new
	    model,
	    color1,
	    color2;

    if(sscanf(params, "k<vehicle>I(1)I(1)", model, color1, color2))
        return SendClientMessage(playerid, -1, !"Введите: /veh [модель] [цвет 1 (опц)] [цвет 2 (опц)]");

    if(!(400 <= model <= 611) || !(0 <= color1 <= 254) || !(0 <= color2 <= 254))
		return SendClientMessage(playerid, -1, !"Введите: /veh [модель] [цвет 1 (опц)] [цвет 2 (опц)]");

    else if(model == 537 || model == 538 || model == 569 || model == 570 || model == 590)
		return SendClientMessage(playerid, -1, !"Создание поездов невозможно.");

	else if(admin_TotalVeh > 100)
		return SendClientMessage(playerid, -1, !"Создано слишком много админ-автомобилей.");

	new Float: X,
		Float: Y,
		Float: Z,
		Float: A,
		vehicleid;

    GetPlayerPos(playerid, X, Y, Z);
	GetPlayerFacingAngle(playerid, A);

	X += 4*floatsin(-A, degrees);
	Y += 4*floatcos(-A, degrees);

	vehicleid = CreateVehicleEx(model, X, Y, Z, A, color1, color2, 60000);

	SetVehicleVirtualWorld(vehicleid, PlayerInfo[playerid][pVirt]);
	LinkVehicleToInterior(vehicleid, PlayerInfo[playerid][pInt]);

	admin_veh{vehicleid} = true;
	Fuell[vehicleid] = 100.0;
	admin_TotalVeh ++;

	format(stringer1, sizeof(stringer1), "[A] %s создал автомобиль %s (MODEL: %d) [Всего: %d]", PlayerInfo[playerid][pNames], VehicleNameS[model - 400], model, admin_TotalVeh);
	return SendAdminMessage(COLOR_LIGHTRED, stringer1);
}
CMD:alldelveh(playerid, params[])
{
	if(AdminInfo[playerid][aLevel] < 7 || !AdminInfo[playerid][aLogin])
		return 1;
		
	else if(!admin_TotalVeh)
		return SendClientMessage(playerid, COLOR_GRAD1, "Отсутствуют созданные админ-автомобили!");
		
	for (new vehicleid = GetVehiclePoolSize(); vehicleid != 0; --vehicleid) {
	    if(!admin_veh{vehicleid}) continue;
	    admin_veh{vehicleid} = false;
		DestroyVehicleEx(vehicleid);
	}

	format(stringer1, sizeof(stringer1), "[A] %s удалил все автомобили созданные администраторами (всего %d)", PlayerInfo[playerid][pNames], admin_TotalVeh);
    SendAdminMessage(COLOR_LIGHTRED, stringer1);
    
	admin_TotalVeh = 0;
	return 1;
}
CMD:delveh(playerid, params[])
{
	if(AdminInfo[playerid][aLevel] < 5 || !AdminInfo[playerid][aLogin])
		return 1;
		
	else if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER)
		return SendClientMessage(playerid, COLOR_GRAD1, "Вы должны быть за рулем автомобиля!");

	new
		vehicleid = GetPlayerVehicleID(playerid),
		model = GetVehicleModel(vehicleid);
		
	if(!admin_veh{vehicleid})
		return SendClientMessage(playerid, COLOR_GRAD1, "Эту машину удалять нельзя");
		
	format(stringer1, sizeof(stringer1), "[A] %s удалил автомобиль %s (MODEL: %d) [Всего: %d]", PlayerInfo[playerid][pNames], VehicleNameS[model - 400], model, admin_TotalVeh);
	SendAdminMessage(COLOR_LIGHTRED, stringer1);

	admin_veh{vehicleid} = false;
	DestroyVehicleEx(vehicleid);
	admin_TotalVeh --;
	
	return true;
}
