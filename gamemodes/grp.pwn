	if(sscanf(params,"d",params[0])) return	SendClientMessage(playerid, COLOR_WHITE, "Введите: /uinvite [id]");
	if(!IsPlayerConnected(params[0])) return 1;
	if(PlayerInfo[playerid][pMember] != PlayerInfo[params[0]][pMember]) return SendClientMessage(playerid,COLOR_WHITE, "Игрок состоит не в вашей фракции");
	if(PlayerInfo[params[0]][pUnit]) return SendClientMessage(playerid,COLOR_WHITE, "Игрок состоит в подразделении");
	if(!ProxDetectorS(10.0, playerid, params[0]) || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0])) return SendClientMessage(playerid,COLOR_WHITE,"Вы далеко друг от друга");
	if(strcmp(Unit_Leader[PlayerInfo[playerid][pMember]-1][PlayerInfo[playerid][pUnit]-1], PlayerInfo[playerid][pNames])) return SendClientMessage(playerid,COLOR_WHITE,"Вам недоступна данная команда");
	if(SelectionStart[playerid] != -1) return SendClientMessage(playerid, 0xFF6600AA, "У Вас уже есть активное предложение, отзовите его, используя /cansel");

	SelectionStart[playerid] = params[0];
	SelectionWait[params[0]] = playerid;
	SelectionType[playerid] = 2;
	SendMes(playerid, 0x1e90ffAA, "Вы предложили игроку %s вступить в подразделение \"%s\". Что бы отозвать предложение, используйте {1e90ff}/cansel", PlayerInfo[params[0]][pNames], Unit_Name[PlayerInfo[playerid][pMember]-1][PlayerInfo[playerid][pUnit]-1]);
	SendMes(params[0], 0x1e90ffAA, "%s {1e90ff}предлагает Вам стать лидером подразделения \"%s\"", PlayerInfo[playerid][pNames], Unit_Name[PlayerInfo[playerid][pMember]-1][PlayerInfo[playerid][pUnit]-1]);
	return SendClientMessage(params[0], COLOR_WHITE, "Нажмите {03c03c}Y {FFFFFF}чтобы согласиться или {ff7518}N {FFFFFF}для отказа.");
}
