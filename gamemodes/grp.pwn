CMD:vipad(playerid, params[])
{
	if(isnull(params)) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /v(ip)ad [текст]");
	if(PlayerInfo[playerid][pMuted] > 0) return SendClientMessage(playerid, TEAM_CYAN_COLOR, "У Вас бан чата!");
	if(PlayerInfo[playerid][pDonateRank] == 0) return SendClientMessage(playerid,COLOR_WHITE,"У вас нет VIP аккаунта!");
	if(gVip[playerid][vipTime] > 0) return SendClientMessage(playerid, COLOR_GREY, "Вы уже подали объявление на публикацию. Ожидайте...");
	if(IsIpFound(params) || CheckString(params))
	{
		Reklama(playerid,params);
		return true;
	}
	switch(PlayerInfo[playerid][pDonateRank])
	{
	case 1:
		{
			VipNews[playerid] ++;
			if(VipNews[playerid] >= 3) return SendClientMessage(playerid,COLOR_WHITE,"Вы исчерпали свой лимит для подачи обьявлений!");
		}
	case 2:
		{
			if(VipNews[playerid] >= 7) return SendClientMessage(playerid,COLOR_WHITE,"Вы исчерпали свой лимит для подачи обьявлений!");
		}
	}
	strmid(gVip[playerid][vipSender], PlayerInfo[playerid][pNames], 0, strlen(PlayerInfo[playerid][pNames]), 255);
	gVip[playerid][vipID] = playerid;
	strmid(gVip[playerid][vipText], params, 0, strlen(params), 255);
	gVipTime += 30;
	gVip[playerid][vipTime] = gVipTime;
	gVip[playerid][vipNumber] = PlayerInfo[playerid][pPnumber];
	gVip[playerid][vipChek] = true;
	SendMes(playerid, COLOR_GREY, "Ваше объявление успешно доставлено в службу проверки персональных объявлений. Примерное время ожидания публикования: %i секунд.",gVip[playerid][vipTime]);
	FracBank[0][fLsnews] += strlen(params)*addd;
	PlayerInfo[playerid][pCash] -= strlen(params)*addd;
	format(stringer,sizeof(stringer), "~r~-$%d", strlen(params)*addd);
	GameTextForPlayer(playerid, stringer, 5000, 1);
	return true;
}
