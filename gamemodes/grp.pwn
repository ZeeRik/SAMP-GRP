	SetPlayerColor(targetid, 0xFFFFFF20);
	SetPlayerSkinEx(targetid, PlayerInfo[targetid][pChar]);

	UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET member = 0, rank = 0, leader = 0, duty = 0, reprimands = 0, unit = 0, frac_mute = 0 WHERE id = %d", PlayerInfo[targetid][pID]);

	format(stringer1, sizeof(stringer1), "уволил из организации %s, с причиной: %s",GetPlayerFracName[PlayerInfo[targetid][pMember]], reason);
