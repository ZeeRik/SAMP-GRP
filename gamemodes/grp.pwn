			DeletePVar(playerid, "CurrentPlayerDrug");
			ClearPlayerDrugEffects(playerid);
		}
	}
	if(GetPVarInt(playerid, "DrugsSound") >= 1) {
		SetPVarInt(playerid, "DrugsSound", GetPVarInt(playerid, "DrugsSound") -1);
