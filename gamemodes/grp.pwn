CMD:setskin(playerid, params[])
{
	if(AdminInfo[playerid][aLevel] < 5 || !AdminInfo[playerid][aLogin]) return true;

	extract params -> new player:targetid, act, skinid; else
        return SendClientMessage(playerid, -1, !"Введите: /setskin [ID игрока] [1 - статический | 2 - фракционный | 3 - временный] [id скина]");

	if(!IsPlayerConnected(targetid) || !PlayerInfo[targetid][pID])
		return SendClientMessage(playerid, COLOR_GREY, "Игрок не авторизирован на сервере!");

	else if(!(0 < skinid < 312) || skinid == 74)
		return SendClientMessage(playerid, COLOR_GREY, "Неправильный ID скина!");

	else if(!(0 < act < 4))
		return SendClientMessage(playerid, COLOR_GREY, "Неверное значение!");

	if(act == 1) PlayerInfo[targetid][pChar] = skinid;
	else if(act == 2) PlayerInfo[targetid][pModel] = skinid;
	SetPlayerSkinEx(targetid, skinid);

	if(act < 3) UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `%s` = '%d' WHERE `id` = '%d'", (act == 1) ? ("char_skin") : ("model"), skinid, PlayerInfo[targetid][pID]);

	SendMes(targetid, COLOR_REDD, "Администратор %s изменил Ваш скин на ID: %d", PlayerInfo[playerid][pNames], skinid);
	SendMes(playerid, 0x6495EDFF, "Вы изменили скин %s на номер: %d", PlayerInfo[targetid][pNames], skinid);
	return true;
}
