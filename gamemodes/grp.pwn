forward OnCheatDetected(playerid, const ip_address[], type, code);
public OnCheatDetected(playerid, const ip_address[], type, code)
{
    if(type)
	{
		BlockIpAddress(ip_address, 0);
        AC_CODE_COUNT[code] ++;

		format(stringer1, sizeof(stringer1), "<AC-BAN-IP> IP-адрес: {FFFFFF}%s {FF4500}был заблокирован: {FFFFFF}%s [code: #%03d].", ip_address, AC_CODE[code], code + 1);
		return SendAdminMessage(0xFF4500AA, stringer1);
	}

	if(playerPBot[playerid] != -1)
		return 1;

	AC_CODE_COUNT[code] ++;
    switch(code)
	{
		case 5, 6, 11, 14, 22, 32: return 1;
		case 40: SendClientMessage(playerid, COLOR_LIGHTRED, MAX_CONNECTS_MSG);
		case 41: SendClientMessage(playerid, COLOR_LIGHTRED, UNKNOWN_CLIENT_MSG);
		default:
		{
			if(AC_CODE_TYPE[code] == 1) {
				AddCheater(playerid);
				return 1;
			}

			SendMes(playerid, COLOR_LIGHTRED, "[Античит] Вы были кикнуты в связи с подозрением в читерстве. Код события: #%03d", code + 1);
			SendClientMessage(playerid, COLOR_LIGHTRED, !"Примечание: К ложным определениям античита могут привести следующие факторы:");
			SendClientMessage(playerid, COLOR_LIGHTRED, !"Высокий пинг, слабое интернет-соединение, проблемы на сервере, проблемы в игре");

			format(stringer1, sizeof(stringer1), "<AC-KICK> {FFFFFF}%s[%d] {FF4500}был кикнут за чит-программы: {FFFFFF}%s [code: #%03d].", PlayerInfo[playerid][pNames], playerid, AC_CODE[code], code + 1);
			SendAdminMessage(0xFF4500AA, stringer1);

			if(code == 15 || code == 16 || code == 17) {
				for (new i = 0; i < 13; i++) PlayerInfo[playerid][Weapon][i] = PlayerInfo[playerid][Ammo][i] = 0;
			}
		}
	}

	AntiCheatKickWithDesync(playerid, code);

    return 1;
}
