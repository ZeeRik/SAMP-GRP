stock GoServer(playerid)
{
    if(gPlayerLogged{playerid} == 0 && PlayerInfo[playerid][pCheckIp] && strcmp(PlayerInfo[playerid][pIP], PlayerInfo[playerid][pGetonIP],false) != 0)
	{
	    SendClientMessage(playerid, COLOR_WHITE, "Чтобы завершить стадию авторизации, вам необходимо пройти проверку.");
        SendClientMessage(playerid, COLOR_WHITE, "Для этого кликайте на цифры, которые вы указали в коде безопасности.");
		return Capcha(playerid);
	}
	if(PlayerInfo[playerid][pVipTime] > 0 && PlayerInfo[playerid][pDonateRank] > 0)
	{
        new year, month, day, curdate11 = getdate(year,month,day), ostalos=PlayerInfo[playerid][pVipTime]-curdate11;
		if(ostalos <= 0)
		{
			SendClientMessage(playerid, 0x00b953ff, "Срок действия вашего VIP аккаунта истек.");
			PlayerInfo[playerid][pDonateRank] = 0;
			PlayerInfo[playerid][pVipTime] = 0;
			PlayerInfo[playerid][pVipData] = 0;
			UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `vip_time` = '%d', `vip_date` = '%d', `donate_rank` = '%d' WHERE `id` = '%d'", PlayerInfo[playerid][pVipTime], PlayerInfo[playerid][pVipData], PlayerInfo[playerid][pDonateRank], PlayerInfo[playerid][pID]);
		}
	}
	if(PlayerInfo[playerid][pDonateRank] > 0)
	{
		SendClientMessage(playerid, 0xeac700aa,"Здравствуйте VIP игрок!");
		format(stringer1, sizeof(stringer1), "~w~welcome~n~~b~%s", PlayerInfo[playerid][pNames]);
		GameTextForPlayer(playerid, stringer1, 5000, 1);
	}
	else GameTextForPlayer(playerid, "~w~welcome", 5000, 1);
	if(PlayerInfo[playerid][pHelper] > 0) SendMes(playerid,COLOR_WHITE,"Вы вошли хелпером, %d уровня.",PlayerInfo[playerid][pHelper]);
	if(PlayerInfo[playerid][pPhousekey] != 255 && caridhouse[playerid] == INVALID_VEHICLE_ID)
	{
	    if(PlayerInfo[playerid][pCar] == 0) PlayerInfo[playerid][pCar] = 462;
		if(PlayerInfo[playerid][pCar2] == 0) PlayerInfo[playerid][pCar2] = 462;
		
		new house = PlayerInfo[playerid][pPhousekey];
		caridhouse[playerid] = CreateVehicleEx(PlayerInfo[playerid][pCar], 2485.8762,-2561.1030,2500.8086,360.0000, cColor[playerid][0][0], cColor[playerid][0][1],7200, 0);
		VehicleInfo[caridhouse[playerid]][vLock] = 1;
		LinkVehicleToInterior(caridhouse[playerid], 1);
		SetVehicleVirtualWorld(caridhouse[playerid], house + 50);
		Fuell[caridhouse[playerid]] = PlayerInfo[playerid][pFuelcar] + 20.0;
		AddTune(playerid, 0);
		SetPVarInt(playerid, "CarNomer", 0);
	}
