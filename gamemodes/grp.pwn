    return SendAdminMessage(0xFF0000FF, stringer1);
}
publics: Sub_VigDate(playerid, tmp[], reason[], frid, type)
{
	if(!cache_num_rows())
        return SendMes(playerid, COLOR_GREY, "Аккаунт %s не найден в базе данных", tmp);

	new
	    leader,
		vigs;

	cache_get_value_name_int(0, "leader", leader);
	cache_get_value_name_int(0, "reprimands", vigs);

	if(!leader)
		return SendClientMessage(playerid, COLOR_WHITE, !"Указанный игрок не является лидером!");

	if(frid != -1) {
		new subLeader = SubCheck(frid, leader);

		if(!subLeader)
			return SendClientMessage(playerid, COLOR_GREY, !"У вас нет полномочий на управление данной организацией");
	}

	if(type == 1)
	{
		if( ++ vigs >= 3) {

  		    format(stringer1, sizeof(stringer1), "%s %s [%d] снял лидера фракции %s: %s (3/3). Причина: %s", (AdminInfo[playerid][aLevel]) ? ("Администратор") : (fRankName[PlayerInfo[playerid][pMember]-1][PlayerInfo[playerid][pRank]-1]), PlayerInfo[playerid][pNames], playerid, GetPlayerFracName[leader], tmp, reason);
            SendClientMessageToAll(COLOR_LIGHTRED, stringer1);
  			format(stringer1, sizeof(stringer1), "снял лидеры фракции %s по причине (3/3): %s", GetPlayerFracName[leader], reason);
  			Admin_Log(playerid, stringer1, tmp);
  			UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `leader` = '0', `member` = '0', `rank` = '0', `reprimands` = '0', `duty` = '0' WHERE `name` = '%s'", tmp);
		}
		else
		{
  	        format(stringer1, sizeof(stringer1), "%s %s [%d] выдал выговор лидеру фракции %s: %s (%d/3). Причина: %s", (AdminInfo[playerid][aLevel]) ? ("Администратор") : (fRankName[PlayerInfo[playerid][pMember]-1][PlayerInfo[playerid][pRank]-1]), PlayerInfo[playerid][pNames], playerid, GetPlayerFracName[leader], tmp, vigs, reason);
  		    SendClientMessageToAll(COLOR_LIGHTRED, stringer1);
  		    format(stringer1, sizeof(stringer1), "выдал выговор лидеру фракции %s (%d/3) по причине: %s", GetPlayerFracName[leader],vigs, reason);
  		    Admin_Log(playerid, stringer1, tmp);
		    UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `reprimands` = '%d' WHERE `name` = '%s'", vigs, tmp);
		}
	}

	else
	{
		vigs --;
	    format(stringer1, sizeof(stringer1), "%s %s [%d] снял выговор лидеру фракции %s: %s (%d/3). Причина: %s", (AdminInfo[playerid][aLevel]) ? ("Администратор") : (fRankName[PlayerInfo[playerid][pMember]-1][PlayerInfo[playerid][pRank]-1]), PlayerInfo[playerid][pNames], playerid, GetPlayerFracName[leader], tmp, vigs, reason);
  		SendClientMessageToAll(COLOR_LIGHTRED, stringer1);
  		format(stringer1, sizeof(stringer1), "снял выговор лидеру фракции %s (%d/3) по причине: %s", GetPlayerFracName[leader], vigs, reason);
  		Admin_Log(playerid, stringer1, tmp);
