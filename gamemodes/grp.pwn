CMD:crashcuff(playerid, params[])
{
	if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /crashcuff [id]");
	if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует");
	if(PlayerInfo[playerid][pOtmbI4ka] == 0) return SendClientMessage(playerid, COLOR_GREY, "У вас нет отмычек");
	if(PlayerCuffed[params[0]] == 0) return SendClientMessage(playerid, COLOR_GREY, "Игрок не в наручниках");
	if(crashcuff[playerid] > 0) return SendClientMessage(playerid, COLOR_GREY, "Команду можно использовать раз в 30 секунд");
	if (ProxDetectorS(8.0, playerid, params[0]))
	{
		new randomm;
		switch(PlayerInfo[params[0]][pDonateRank])
		{
		case 1: { randomm = random(12)+1; }
		case 2: { randomm = random(11)+1; }
		case 3: { randomm = random(10)+1; }
		default: { randomm = random(13)+1; }
		}
		switch(randomm)
		{
		case 1..3:
			{
				TogglePlayerControllable(params[0], 1);
				SetPlayerSpecialAction(params[0],SPECIAL_ACTION_NONE);
				RemovePlayerAttachedObject(params[0],0);
				PlayerCuffed[params[0]] = 0;
				crashcuff[playerid] = 30;
				PlayerInfo[playerid][pOtmbI4ka]--;
				format(stringer1, sizeof(stringer1), "Игрок %s скинул с себя наручники.", PlayerInfo[params[0]][pNames]);
				ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
			}
		default:
			{
				SendClientMessage(playerid, COLOR_GREY, "Вам не удалось скинуть наручники.");
				PlayerInfo[playerid][pOtmbI4ka]--;
				crashcuff[playerid] = 30;
			}
		}
	}
	else return SendClientMessage(playerid, COLOR_GREY, "Игрок не рядом с вами.");
	return true;
}
CMD:givebil(playerid, params[])
{
    if(PlayerInfo[playerid][pLeader] != 3 && PlayerInfo[playerid][pLeader] != 19 && (AdminInfo[playerid][aLevel] < 3 || !AdminInfo[playerid][aLogin]))
	    return SendClientMessage(playerid, COLOR_WHITE, !"У вас нет прав на выдачу военного билета.");

    extract params -> new player:targetid; else
	    return SendClientMessage(playerid, -1, !"Введите: /givebil [ID]");

	if(!IsPlayerConnected(targetid) || targetid == playerid)
	    return SendClientMessage(playerid, COLOR_WHITE, !"Ошибка: Игрока не существует или это Вы");

    if(PlayerInfo[targetid][pVoenBill] != 0)
	    return SendClientMessage(playerid, COLOR_WHITE, !"У игрока уже имеется военный билет.");

    if(!AdminInfo[playerid][aLevel] && GetDistanceBetweenPlayers(playerid, targetid) > 2)
	    return SendClientMessage(playerid, COLOR_WHITE, !"Игрок должен находится рядом с вами.");

    SendMes(targetid, -1, "%s %s[%d] выдал вам военный билет.", (AdminInfo[playerid][aLevel]) ? ("Администратор") : (fRankName[PlayerInfo[playerid][pMember]-1][PlayerInfo[playerid][pRank]-1]), PlayerInfo[playerid][pNames], playerid);
	SendMes(playerid, -1, "Вы выдали военный билет %s[%d]", PlayerInfo[targetid][pNames], targetid);

	PlayerInfo[targetid][pVoenBill] = 1;
	UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `voenbill` = '1' WHERE `id` = '%d'",PlayerInfo[targetid][pID]);

    return true;
}
