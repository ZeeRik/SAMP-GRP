	    else if(PlayerInfo[playerid][pFacePass] == 4) SendClientMessage(params[0], COLOR_YELLOW2, "Фракция: Правительство");
		SendClientMessage(params[0], COLOR_YELLOW2, "Работа: Безработный");
		SendClientMessage(params[0], COLOR_YELLOW2, "Разрешение на оружие: Есть");
		SendMes(params[0], COLOR_YELLOW2, "Преступлений: %d",PlayerInfo[playerid][pKills]);
		SendMes(params[0], COLOR_YELLOW2, "Законопослушность: %d",PlayerInfo[playerid][pZakonp]);
		SendClientMessage(params[0], COLOR_YELLOW2, "Военный билет: {33AA33}Служил");
	}
	SendClientMessage(params[0],COLOR_GREY, "=============================");
	format(stringer1,sizeof(stringer1), "%s показал(а) свой паспорт %s'у", PlayerInfo[playerid][pNames], PlayerInfo[params[0]][pNames]);
	SetPlayerChatBubble(playerid,stringer1,COLOR_PURPLE,30.0,10000);
	format(stringer1, sizeof(stringer1), "%s показал(a) свой паспорт", PlayerInfo[playerid][pNames]);
	ProxDetector(playerid, 10.0, COLOR_PURPLE, stringer1);
	return true;
