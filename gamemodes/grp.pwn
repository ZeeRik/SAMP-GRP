CMD:iinvite(playerid, params[])
{
    if(PlayerInfo[playerid][pOdet] == 0 || PlayerInfo[playerid][pMember] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!");
    if(PlayerInfo[playerid][pRank] < gFractionRank[PlayerInfo[playerid][pMember]-1][0]) return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PlayerInfo[playerid][pMember]-1][0]);
	if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /iinvite [id]");
	if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
	if(IsPlayerInAnyVehicle(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Игрок в машине.");
	if(!ProxDetectorS(10.0, playerid, params[0]) || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0])) return SendClientMessage(playerid,COLOR_WHITE,"Вы далеко друг от друга");
	if(PlayerInfo[params[0]][pLeader] != 0) return SendClientMessage(playerid, COLOR_GREY, "Нельзя пригласить лидера.");
	switch(PlayerInfo[playerid][pMember])
	{
		case 1, 10, 21: if(!IsAArm(params[0]) || PlayerInfo[params[0]][pRank] < 3) return SendClientMessage(playerid, COLOR_WHITE, "Ранг игрока недостаточен для перевода");
		case 2: if((!IsAArm(params[0]) && !IsACop(params[0])) || PlayerInfo[params[0]][pRank] < 5) return SendClientMessage(playerid, COLOR_WHITE, "Ранг игрока недостаточен для перевода");
		case 25: if((!IsAArm(params[0]) && !IsACop(params[0])) || PlayerInfo[params[0]][pRank] < 4) return SendClientMessage(playerid, COLOR_WHITE, "Ранг игрока недостаточен для перевода");
		default: return SendClientMessage(playerid,COLOR_GREY,"Вам не доступна данная функция.");
	}
	format(stringer1,sizeof(stringer1),"{FFFFFF}%s предлагает Вам вступить в организацию: {FF6F00}%s{FFFFFF}\n\nНажмите \"Да\" для согласия или \"Нет\" для отказа!",PlayerInfo[playerid][pNames],GetPlayerFracName[PlayerInfo[playerid][pMember]]);
	ShowPlayerDialog(params[0], 76, DIALOG_STYLE_MSGBOX, "{FF6F00}Вступление во фракцию",stringer1,"Да","Нет");
	SendMes(playerid, -1, "Вы предложили {FF6F00}%s{FFFFFF} вступить в организацию {FF6F00}%s",PlayerInfo[params[0]][pNames],GetPlayerFracName[PlayerInfo[playerid][pMember]]);
	SetPVarInt(params[0],"Player",playerid);
	return true;
