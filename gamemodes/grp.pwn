stock SaveOnline(playerid)
{
    if(!PlayerInfo[playerid][online_player])
    	return 1;

	format(mysql_query_, sizeof(mysql_query_),"SELECT id FROM online WHERE account_id = %d AND date = CURDATE()", PlayerInfo[playerid][pID]);
	new Cache:result = mysql_query(connects, mysql_query_);

	if(!cache_num_rows())
		format(mysql_query_, sizeof(mysql_query_),"INSERT INTO online (account_id, date, online_sec, afk_sec) VALUES (%d, CURDATE(), %d, %d)", PlayerInfo[playerid][pID], PlayerInfo[playerid][online_player], PlayerInfo[playerid][afk_online]);
	else
	{
		new ids;
		cache_get_value_name_int(0, "id", ids);
		format(mysql_query_, sizeof(mysql_query_),"UPDATE online SET online_sec = online_sec + %d, afk_sec = afk_sec + %d WHERE id = %d", PlayerInfo[playerid][online_player], PlayerInfo[playerid][afk_online], ids);
	}

	mysql_tquery(connects, mysql_query_);
	cache_delete(result);
	return 1;
}
