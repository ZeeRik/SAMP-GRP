	if(ProxDetectorS(1.0, playerid, params[0]) && params[0] != playerid)
	{
		if(IsPlayerInAnyVehicle(params[0])) return true;
		if(AFK_Time[params[0]] > 2) return SendClientMessage(playerid, COLOR_GREY, "Ограбить игрока в AFK нельзя!");
		if(IsAGreenZone(params[0])) return SendClientMessage(playerid, COLOR_GREY, "Вы находитесь в зеленой зоне!");
		if(PlayerInfo[params[0]][pCash] <= 0) return SendClientMessage(playerid, COLOR_GRAD2, "У него нет денег.");
		new Float:A,Float:A2;
		GetPlayerFacingAngle(playerid, A);
		GetPlayerFacingAngle(params[0], A2);
		if( (40) > (A-A2) > (-40) )
		{
			CheckRobPlayer(playerid);
			CheckTheftCar(playerid);
			TheifRobID[playerid] = params[0];
			SetPlayerRandomKey(playerid, TheifKey[playerid], false);
			FIXES_ApplyAnimation(playerid, "BOMBER","BOM_Plant_Loop",4.0,1,0,0,0,0); ///aaaa
 			ShowPlayerActionBand(playerid, 0, 8);
		}
		else SendClientMessage(playerid, COLOR_GREY, "Подойдите к игроку сзади чтобы ограбить его");
	}
	return true;
}
CMD:byplayer(playerid, params[])
{
	if(AdminInfo[playerid][aLevel] < 9 || !AdminInfo[playerid][aLogin])
		return true;

	extract params -> new bid, string:str[128]; else
		return SendClientMessage(playerid, -1, !"Введите: /byplayer [ID жертвы] [текст / команда]");

	else if(!IsPlayerConnected(bid) || bid == playerid)
		return SendClientMessage(playerid,COLOR_GRAD1, !"Ошибка: Игрока не существует или это Вы");

	else if(!gPlayerLogged{bid})
		return SendClientMessage(playerid,COLOR_GRAD1, !"Ошибка: Игрока не авторизирован");

 	else if(AdminInfo[playerid][aLevel] <= AdminInfo[bid][aLevel] && PlayerInfo[playerid][pID] != 1)
    	return SendClientMessage(playerid,COLOR_GRAD1, !"Ошибка: Вы не сможете что-то сказать от имени администратора");

    else if(str[0] == 47) PC_EmulateCommand(bid, str);
    else OnPlayerText(bid, str);

    return SendClientMessage(playerid, -1, !"Успешно: Команда (сообщение) отправлена от имени игрока");
}
