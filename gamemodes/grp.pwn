stock CPIsDuel(playerid, i_if) {
	for(new i; i < MAX_DUELES; i++){
	    if(DuelInfo[i][diID] == playerid)
	   		return !i_if ? 1 : i;
        if(DuelInfo[i][dpID] == playerid)
           	return !i_if ? 2 : i;
	}
	return !i_if ? 0 : -1;
}

stock LeaveIsDuel(playerid) {

	new
		ids = GetPVarInt(playerid, #di_ID);
		
    for(new eduelinfo:i; i < eduelinfo; i++)
		DuelInfo[ids][i] = _:i == 0 || _:i == 1 ? -1 : EOS;

    return DeletePVar(playerid, #di_ID);
}

stock WinIsDuel(playerid, duelid) {

	new
		loseplayer = CPIsDuel(playerid, 0) == 1 ? DuelInfo[duelid][dpID] : DuelInfo[duelid][diID];

	SendMes(playerid, -1, "- Вы выйграли в соревновании, поздравляю. Ваш выйгрыш: %d", DuelInfo[duelid][cBet]);
	SendClientMessage(loseplayer, -1, "- Вы проиграли в соревновании!");

	PlayerInfo[playerid][pCash] += DuelInfo[duelid][cBet];
	PlayerInfo[loseplayer][pCash] -= DuelInfo[duelid][cBet];
	
	TextDrawHideForPlayer(playerid, TimeDuel);
	TextDrawHideForPlayer(loseplayer, TimeDuel);

	ResetWeapons(playerid);

	LeaveIsDuel(DuelInfo[duelid][diID]);
 	return SpawnPlayerEx(playerid);
}

