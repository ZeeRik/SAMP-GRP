stock SendFamilyMessage(family, color, const string[])
{
	foreach(Player,i)
	{
		if(!IsPlayerConnected(i) || gPlayerLogged{i} == 0) continue;
		if(PlayerInfo[i][pMember] == family || PlayerInfo[i][pLeader] == family) SendClientMessage(i, color, string);
	}
	return true;
}
stock SendDepartamentMessage(color, const string[])
{
	foreach(new i : Player) {

		if(!gPlayerLogged{i})
			continue;

		switch(PlayerInfo[i][pMember])
		{
			case 1,2,3,4, 7, 10, 11, 19, 21, 25,26: {
				if(!PlayerInfo[i][pOdet])
					continue;

				SendClientMessage(i, color, string);
			}
		}
	}
	return 1;
}

stock SendRadioMessage(member, color, const string[])
{
	foreach(Player,i)
	{
	    if(!IsPlayerConnected(i) || gPlayerLogged{i} == 0) continue;
		if(PlayerInfo[i][pMember] == member || PlayerInfo[i][pLeader] == member)
		{
	        switch(PlayerInfo[i][pMember])
		    {
			    case 1,2,3,4, 7, 10, 11, 19, 21, 25,26:
			    {
				    if(PlayerInfo[i][pOdet] == 0) continue;
				    SendClientMessage(i,color,string);
			    }
			    default: SendClientMessage(i, color, string);
		    }
		}
	}
	return true;
}
