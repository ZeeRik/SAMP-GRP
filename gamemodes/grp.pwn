CMD:iwep(playerid, params[])
{
    if(AdminInfo[playerid][aLevel] < 2 || !AdminInfo[playerid][aLogin]) return true;

    new targetid,
        weapon_id,
		weapon_name[19],
		ammo,
	    weapon_list;

    if(sscanf(params, "u", targetid))
        return SendClientMessage(playerid, -1, !"Введите /iwep [игрок]");

    if(!IsPlayerConnected(targetid) || !PlayerInfo[targetid][pID])
		return SendClientMessage(playerid, COLOR_GREY, !"Игрок не авторизирован на сервере!");

    stringer[0] = EOS;
    for (new i; i <= 12; i++)
    {
        GetPlayerWeaponData(targetid, i, weapon_id, ammo);
        if(weapon_id == 0) continue;
        else
        {
            GetWeaponName(weapon_id, weapon_name, 19);
            format(stringer, sizeof(stringer), "%s\t%i\n%s", weapon_name, ammo, stringer);
            weapon_list ++;
        }
    }
    if(!weapon_list) return SendMes(playerid, COLOR_GREY, !"У %s[%d] отсуствует какое-либо оружие.", PlayerInfo[targetid][pNames], targetid);
    strins(stringer, "Оружие\tКоличество\n", 0);

    format(stringer1, sizeof(stringer1), "Оружие %s", PlayerInfo[targetid][pNames]);
    return ShowPlayerDialog(playerid, 0, DIALOG_STYLE_TABLIST_HEADERS, stringer1, stringer, !"Закрыть", "");
}
CMD:undemote(playerid, params[])
{
	if(!PlayerInfo[playerid][pOdet]) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!");
	new frid = GetSubfractionID(playerid);
	if(frid == -1) return SendClientMessage(playerid, COLOR_WHITE, "У Вас нет прав на использование этой команды.");
	if(sscanf(params, "dd",params[0], params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /undemote [id] [ранг]");
	if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid,COLOR_GRAD1, "Ошибка: Игрока не существует или это Вы");
	if(PlayerInfo[params[0]][pLeader] != 0) return SendClientMessage(playerid,COLOR_GRAD1, "Ошибка: Данный игрок лидер.");

	if(PlayerInfo[playerid][pLeader] == 2 || (PlayerInfo[playerid][pMember] == 26 && PlayerInfo[playerid][pRank] == 8)) {
		switch(PlayerInfo[params[0]][pMember]) {
    		case 1, 10, 21: if(params[1] > 14 || params[1] < 0) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 0 и больше 14!");
     		case 25: if(params[1] > 9 || params[1] < 0) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 0 и больше 9!");
     		default: return SendClientMessage(playerid, COLOR_GREY, "У Вас нет прав для управления этой фракции.");
		}
	}
	else if(PlayerInfo[playerid][pMember] == 26) {
		if(PlayerInfo[playerid][pRank] == 7) {
			if(PlayerInfo[params[0]][pMember] == 3 || PlayerInfo[params[0]][pMember] == 19) {
				if(params[1] > 14 || params[1] < 0) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 0 и больше 14!");
			}
			else return SendClientMessage(playerid, COLOR_GREY, "У Вас нет прав для управления этой фракции.");
		}
		else if(PlayerInfo[playerid][pRank] == 6) {
			if(PlayerInfo[params[0]][pMember] != 4) return SendClientMessage(playerid, COLOR_GREY, "У Вас нет прав для управления этой фракции.");
		 	if(params[1] > 9 || params[1] < 0) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 0 и больше 9!");
		}
		else if(PlayerInfo[playerid][pRank] == 5) {
			if(PlayerInfo[params[0]][pMember] == 16) if(params[1] > 12 || params[1] < 0) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 0 и больше 12!");
			else return SendClientMessage(playerid, COLOR_GREY, "У Вас нет прав для управления этой фракции.");
		}
		else if(PlayerInfo[playerid][pRank] >= 9) {
			switch(PlayerInfo[params[0]][pMember])
    		{
    			case 1,10,3,19,21: if(params[1] > 14 || params[1] < 0) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 0 и больше 14!");
     			case 2,4,11,25,7: if(params[1] > 9 || params[1] < 0) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 0 и больше 9!");
     			case 16: if(params[1] > 12 || params[1] < 0) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 0 и больше 12!");
     			default: return SendClientMessage(playerid, COLOR_GREY, "У Вас нет прав для управления этой фракции.");
    		}
		}
		else return SendClientMessage(playerid, COLOR_GREY, "У Вас нет прав для управления этой фракции.");
	}
	else return SendClientMessage(playerid, COLOR_GREY, "У Вас нет прав для управления этой фракции.");

    if(params[1] > 0)
    {
        if(PlayerInfo[params[0]][pLeader]) PlayerInfo[params[0]][pLeader] = 0;
		PlayerInfo[params[0]][pRank] = params[1];
	    UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `leader` = '%d', `rank` = '%d' WHERE `id` = '%d'", PlayerInfo[params[0]][pLeader], PlayerInfo[params[0]][pRank], PlayerInfo[params[0]][pID]);
	    SendMes(params[0], 0x6495EDFF, "Вы были повышены/понижены до %i ранга Министром %s",params[1], PlayerInfo[playerid][pNames]);
	    SendMes(playerid, 0x6495EDFF, "Вы повысили/понизили %s до %i ранга.", PlayerInfo[params[0]][pNames],params[1]);
	    SetPlayerStraps(params[0]);
	}
	else
	{
	    ResetWeapons(params[0]);
	    PlayerInfo[params[0]][pRank] = 0;
		SetPlayerStraps(params[0], 0);
	    PlayerInfo[params[0]][pLeader] = 0;
		PlayerInfo[params[0]][pMember] = 0;
		PlayerInfo[params[0]][pUnit] = 0;
		PlayerInfo[params[0]][pVigs] = 0;
		SetPlayerArmour(params[0],0);
		SetPlayerColor(params[0], 0xFFFFFF20);
		SetPlayerSkinEx(params[0], PlayerInfo[params[0]][pChar]);
		SendClientMessage(params[0], COLOR_BLUE, "Вы были уволены Министром.");
		SendClientMessage(params[0], 0x6495EDFF, "Теперь вы снова гражданский...");
		SendClientMessage(playerid,COLOR_GREY,"Вы успешно уволили данного игрока");
		PlayerInfo[params[0]][pOdet] = 0;
		format(stringer, sizeof(stringer), "UPDATE `"TABLE_ACCOUNT"` SET `leader` = '0', `member` = '0', `rank` = '0', `duty` = '0', `reprimands` = '0', `unit` = '0' WHERE `id` = '%d'",PlayerInfo[params[0]][pID]);
		mysql_tquery(connects, stringer, "", "");
	}
	return true;
}
CMD:cansel(playerid)
{
	if(SelectionStart[playerid] == -1) return SendClientMessage(playerid, 0xFF6600AA, "У Вас нет активных предложений, которые можно отозвать.");

    if(IsPlayerConnected(SelectionStart[playerid]) && playerid == SelectionWait[SelectionStart[playerid]]) {
		SelectionWait[SelectionStart[playerid]] = -1;
		SendMes(SelectionStart[playerid], 0xFF6600AA, "%s забрал свое предложение назад.",PlayerInfo[playerid][pNames]);
	}
	SendClientMessage(playerid, 0xFF6600AA, "Вы забрали свое активное предложение");
	SelectionStart[playerid] = -1;

	if(SelectionType[playerid] == 1) DeletePVar(playerid,"NewFrid");
	SelectionType[playerid] = -1;
	return true;
}
CMD:newleader(playerid, params[])
{
    if(!PlayerInfo[playerid][pOdet])
	    return SendClientMessage(playerid,COLOR_WHITE,"Вы не на работе!");

	new frid = GetSubfractionID(playerid);
	if(frid == -1)
	    return SendClientMessage(playerid, COLOR_WHITE, "У Вас нет прав на использование этой команды.");

	if(sscanf(params, "d",params[0]))
	    return SendClientMessage(playerid, COLOR_WHITE, "Введите: /newleader [id]");

	if(!IsPlayerConnected(params[0]) || params[0] == playerid)
	    return SendClientMessage(playerid,COLOR_GRAD1, "Ошибка: Игрока не существует или это Вы");

	if(PlayerInfo[params[0]][pLeader])
	    return SendClientMessage(playerid,COLOR_WHITE, "Игрок уже состоит в организации!");

	if(PlayerInfo[params[0]][pWarns])
	    return SendClientMessage(playerid, -1, "У игрока есть предупреждение");

	if(!ProxDetectorS(10.0, playerid, params[0]) || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0]))
	    return SendClientMessage(playerid,COLOR_WHITE,"Вы далеко друг от друга");

	if(SelectionStart[playerid] != -1)
	    return SendClientMessage(playerid, 0xFF6600AA, "У Вас уже есть активное предложение, отзовите его, используя /cansel");

	for(new i; i < 4; i++) {
		if(!gSubfractionName[frid][i]) break;
		if(!i) format(stringer, sizeof(stringer),"%d. %s", i+1, GetPlayerFracName[gSubfractionName[frid][i]]);
		else format(stringer,sizeof(stringer),"%s\n%d. %s", stringer, i+1, GetPlayerFracName[gSubfractionName[frid][i]]);
	}

	ShowPlayerDialog(playerid, 5239, 2, "{33AA33}Выберите подфракцию:", stringer, "Назначить", "Отмена");
	SelectionStart[playerid] = params[0];
	SelectionType[playerid] = 1;
	SetPVarInt(playerid, "NewFrid", frid);
	return true;
}
CMD:wbook(playerid, params[])
{
	new
		targetid;

