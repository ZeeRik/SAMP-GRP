CMD:reset(playerid, params[])
{
	if (AdminInfo[playerid][aLevel] < 7 || !AdminInfo[playerid][aLogin]) return true;
	if(sscanf(params, "ddd",params[0], params[1], params[2])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /reset [ид] [1 - донат, 2 - деньги, 3 - банк] [значение]");
	if(!IsPlayerConnected(params[0])) return true;
	switch(params[1]) {
		case 1:
		{
			if(params[2] <= 0 || params[2] > PlayerInfo[params[0]][pDonatemoney2]) return SendMes(playerid, -1, "Неверное значение (от 1 до %d)", PlayerInfo[params[0]][pDonatemoney2]);
	    	SendMes(playerid, COLOR_WHITE, "Вы аннулировали %s донат очков игроку %s.", formatInt(params[2]), PlayerInfo[params[0]][pNames]);
	    	PlayerInfo[params[0]][pDonatemoney2] -= params[2];
			SendClientMessage(params[0],COLOR_LIGHTRED,"Ваши донат средства были аннулированы, за нарушение правил сервера!");
		}
		case 2:
		{
            if(params[2] <= 0 || params[2] > PlayerInfo[params[0]][pCash]) return SendMes(playerid, -1, "Неверное значение (от 1 до %d)", PlayerInfo[params[0]][pCash]);
	    	SendMes(playerid, COLOR_WHITE, "Вы аннулировали %s наличных вирт игроку %s.", formatInt(params[2]), PlayerInfo[params[0]][pNames]);
	    	PlayerInfo[params[0]][pCash] -= params[2];
			SendClientMessage(params[0],COLOR_LIGHTRED,"Ваши наличные средства были аннулированы, за нарушение правил сервера!");
		}
		case 3:
		{
            if(params[2] <= 0 || params[2] > PlayerInfo[params[0]][pBank]) return SendMes(playerid, -1, "Неверное значение (от 1 до %d)", PlayerInfo[params[0]][pBank]);
	    	SendMes(playerid, COLOR_WHITE, "Вы аннулировали %s вирт в банке игроку %s.",formatInt(params[2]), PlayerInfo[params[0]][pNames]);
	    	PlayerInfo[params[0]][pBank] -= params[2];
			SendClientMessage(params[0],COLOR_LIGHTRED,"Ваши банковские средства были аннулированы, за нарушение правил сервера!");
		}
		default: return SendClientMessage(playerid,COLOR_LIGHTRED,"Неверное значение! Доступные: [1 - донат, 2 - деньги, 3 - банк]");
	}

	format(stringer1, sizeof(stringer1), "< ALog: [%d LVL] > %s (%i) аннулировал игроку %s (%i) игровое имущество (пункт №%d | значение: %s)", AdminInfo[playerid][aLevel], PlayerInfo[playerid][pNames], playerid, PlayerInfo[params[0]][pNames], params[0], params[1], formatInt(params[2]));
    return SendAdminMessage(0xFF0000FF, stringer1);
}
