CMD:subvig(playerid, params[])
{
    new
	    frid = GetSubfractionID(playerid),
		nicks [24];

    if((AdminInfo[playerid][aLevel] < 6 || !AdminInfo[playerid][aLogin]) && (frid == -1 || !PlayerInfo[playerid][pOdet]))
	    return SendClientMessage(playerid, COLOR_GREY, !"У вас нет прав на выдачу выговоров");

    else if(sscanf(params, "s[24]s[100]", nicks, params[0]))
	    return SendClientMessage(playerid, -1, !"Используйте: /subvig [ник] [причина]");

	else if(AntiBan[playerid] >= 60 && AdminInfo[playerid][aLevel] < 9) {

	    SendClientMessage(playerid, COLOR_GREY, !"[Анти-флуд] Ваши действия заблокированы на 3 минуты");
	    AntiBan[playerid] = 180;
	    return true;
	}
	AntiBan[playerid] += 30;

    new
	    ids = GetPlayerID(nicks);

    if(IsPlayerConnected(ids)) {

        if(playerid == ids)
		    return SendClientMessage(playerid, COLOR_WHITE, !"Невозможно выдать выговор самому себе");

        if(AdminInfo[ids][aLevel] || !PlayerInfo[ids][pLeader])
		    return SendClientMessage(playerid, COLOR_WHITE, !"Игрок не является лидером организации или является администратором");

		if(frid != -1) {
		    new subLeader = SubCheck(frid, PlayerInfo[ids][pLeader]);

		    if(!subLeader)
		        return SendClientMessage(playerid, COLOR_GREY, !"У вас нет полномочий на управление данной организацией");
		}

        if(++ PlayerInfo[ids][pVigs] >= 3) {

            format(stringer1, sizeof(stringer1), "%s %s [%d] снял лидера фракции %s: %s [%d] (3/3). Причина: %s", (AdminInfo[playerid][aLevel]) ? ("Администратор") : (fRankName[PlayerInfo[playerid][pMember]-1][PlayerInfo[playerid][pRank]-1]), PlayerInfo[playerid][pNames], playerid, GetPlayerFracName[PlayerInfo[ids][pLeader]], PlayerInfo[ids][pNames], ids, params[0]);
            SendClientMessageToAll(COLOR_LIGHTRED, stringer1);
  		    format(stringer1, sizeof(stringer1), "снял лидеры фракции %s по причине (3/3): %s", GetPlayerFracName[PlayerInfo[ids][pLeader]], params[0]);
            Admin_Log(playerid, stringer1, nicks);
  		    SetPlayerSkinEx(ids, PlayerInfo[ids][pChar]);
  		    SetPlayerArmour(ids,0);
		    SetPlayerHealthAC(ids,100);
		    ResetWeapons(ids);
		    PlayerInfo[ids][pLeader] = PlayerInfo[ids][pMember] = PlayerInfo[ids][pRank] = PlayerInfo[ids][pVigs] = PlayerInfo[ids][pOdet] = 0;
    	    SetPlayerToTeamColor(ids, PlayerInfo[ids][pMember]);
		    UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `leader` = '0', `member` = '0', `rank` = '0', `reprimands` = '0' , `duty` = '0' WHERE `id` = '%d'", PlayerInfo[ids][pID]);
        }
        else
        {
            format(stringer1, sizeof(stringer1), "%s %s [%d] выдал выговор лидеру фракции %s: %s [%d] (%d/3). Причина: %s", (AdminInfo[playerid][aLevel]) ? ("Администратор") : (fRankName[PlayerInfo[playerid][pMember]-1][PlayerInfo[playerid][pRank]-1]), PlayerInfo[playerid][pNames], playerid, GetPlayerFracName[PlayerInfo[ids][pMember]], PlayerInfo[ids][pNames], ids, PlayerInfo[ids][pVigs], params[0]);
  		    SendClientMessageToAll(COLOR_LIGHTRED, stringer1);
  		    format(stringer1, sizeof(stringer1), "выдал выговор лидеру фракции %s (%d/3) по причине: %s", GetPlayerFracName[PlayerInfo[ids][pMember]],PlayerInfo[ids][pVigs], params[0]);
  		    Admin_Log(playerid, stringer1, nicks);
		    UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `reprimands` = '%d' WHERE `id` = '%d'",PlayerInfo[ids][pVigs], PlayerInfo[ids][pID]);
        }
        return true;
    }

    format(mysql_query_,sizeof(mysql_query_), "SELECT `leader`, `reprimands` FROM `"TABLE_ACCOUNT"` WHERE name = '%s'", nicks);
	mysql_tquery(connects, mysql_query_, "Sub_VigDate", "dssdd", playerid, nicks, params[0], frid, 1);

    return true;
}
