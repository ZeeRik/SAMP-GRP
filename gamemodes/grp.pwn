stock Convert(number) {
    new h = floatround(number / 3600),
        m = floatround((number - h * 3600) / 60),
        s = floatround(number - h * 3600 - m * 60),
        str[30]
	;
    if(h > 0) format(str, sizeof(str), "%d:%02d:%02d", h, m, s);
    else format(str, sizeof(str), "%d:%02d", m, s);
	return str;
}
stock GetSubfractionID(playerid) // система подфракций
{
	switch(PlayerInfo[playerid][pMember])
	{
		case 2: if(PlayerInfo[playerid][pRank] == 10) return 0;
		case 26:
		{
			if(PlayerInfo[playerid][pRank] >= 9) return 3; // президент и вице => мэрия
			else if(PlayerInfo[playerid][pRank] == 8) return 0; // мвд  => силовики
			else if(PlayerInfo[playerid][pRank] == 7) return 1; // оборона  => армия
			else if(PlayerInfo[playerid][pRank] == 6) return 2; // минздрав  => мчс
			else if(PlayerInfo[playerid][pRank] == 5) return 4; // сми => новости лс
		}
	}
	return -1;
}
