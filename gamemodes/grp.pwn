publics: SetAdmin_Vigs(playerid, tmp[], reason[], type)
{
    if(!cache_num_rows())
        return SendMes(playerid, COLOR_GREY, "Аккаунт %s не является администратором", tmp);

	new
	    ids,
	    admin,
		vigs;

	cache_get_value_name_int(0, "admin", ids);
	cache_get_value_name_int(0, "level", admin);
	cache_get_value_name_int(0, "vigs", vigs);

	if(admin >= AdminInfo[playerid][aLevel] && PlayerInfo[playerid][pID] != 1 && PlayerInfo[playerid][pID] != 2)
		return SendClientMessage(playerid, COLOR_WHITE, !"У вас нет прав изменять выговоры данному администратору");

	if(type == 1)
	{
		if(++ vigs >= 3)
	    {
	        format(stringer1, sizeof(stringer1), "Главный администратор %s снял с %s права администратора (3/3). Причина: %s", PlayerInfo[playerid][pNames], tmp, reason);
	        SendAdminMessage(COLOR_LIGHTRED, stringer1);

	        format(stringer1, sizeof(stringer1), "снял права администратора (3/3), по причине: %s", reason);
	        Admin_Log(playerid, stringer1, tmp);

	        format(mysql_query_, sizeof(mysql_query_), "DELETE FROM `"TABLE_ADMIN"` WHERE `admin` = '%d'", ids);
            mysql_tquery(connects, mysql_query_, "", "");
        }
        else
        {
            format(stringer1, sizeof(stringer1), "Главный администратор %s выдал администратору %s выговор (%d/3). Причина: %s", PlayerInfo[playerid][pNames], tmp, vigs, reason);
            SendAdminMessage(COLOR_LIGHTRED, stringer1);

            format(stringer1, sizeof(stringer1), "выдал выговор (%d/3), по причине: %s", vigs, reason);
            Admin_Log(playerid, stringer1, tmp);

            format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ADMIN"` SET `vigs` = '%d' WHERE `admin` = '%d'", vigs, ids);
	        mysql_tquery(connects, mysql_query_, "", "");
	    }
	}

	else
	{
	    if(!vigs)
	    	return SendMes(playerid, COLOR_GREY, "У администратора %s нет действительных выговоров", tmp);

        vigs --;

	    format(stringer1, sizeof(stringer1), "Главный администратор %s снял администратору %s выговор (%d/3). Причина: %s", PlayerInfo[playerid][pNames], tmp, vigs, reason);
		SendAdminMessage(COLOR_LIGHTRED, stringer1);

  		format(stringer1, sizeof(stringer1), "снял выговор (%d/3), по причине: %s", vigs, reason);
  		Admin_Log(playerid, stringer1, tmp);

  		format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ADMIN"` SET `vigs` = '%d' WHERE `admin` = '%d'", vigs, ids);
		mysql_tquery(connects, mysql_query_, "", "");
	}
	return true;
}
