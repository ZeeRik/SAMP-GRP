CMD:capture(playerid, params[])
{
	if(!IsAGang(playerid))
		return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Вы не состоите в банде");

	new h; gettime(h);

	if(h < 9 || h > 23)
		return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Использовать команду можно только с 09:00 до 00:00");

	if(PlayerInfo[playerid][pRank] < 7)
		return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Функция доступна с 7 ранга и выше");

	new faction = PlayerInfo[playerid][pMember];

	if(!IsInAllowed(playerid))
		return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Ваша банда уже участвует в захвате зон");

    if(FrakCD[faction] > 0)
		return SendMes(playerid, COLOR_GREY, "Ваша банда устала. Отдыхайте еще %s минут", Convert(FrakCD[faction]));

	if(gFractionFreeze[faction-1] > gettime())
		return SendClientMessage(playerid, -1, !"{FF2222}[Ошибка] {FFFFFF}Ваша банда заморожена и не может принимать участия в захвате");

    new
        typeCapture;

	if(sscanf(params, !"d", typeCapture) || (typeCapture != 1 && typeCapture != 2))
	{
		SendClientMessage(playerid, -1, !"{FF2222}[Ошибка] {FFFFFF}Введите: /capture [ТИП (1-2)]");
        SendClientMessage(playerid, -1, !"- ТИП 1: Война по \"{FF2222}убийствам{FFFFFF}\"");
        return SendClientMessage(playerid, -1, !"- ТИП 2: Война по \"{FF2222}кол-ву человек{FFFFFF}\"");
	}
	
	for(new i = 1; i <= TOTALGZ; i++)
	{
		if(PlayerToKvadrat(playerid, GZInfo[i][gCoords][0], GZInfo[i][gCoords][1], GZInfo[i][gCoords][2], GZInfo[i][gCoords][3])
			&& ZoneOnBattle[i] == 0)
		{
		    switch(i)
			{
				case 42, 44, 69, 87, 102:
					return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Эту территорию нельзя захватить!");
			}

			if(IsPlayerInBandOnline(faction) < 2)
				return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}В вашей банде мало игроков онлайн!");

			if(IsPlayerInBandOnline(GZInfo[i][gFrak]) < 2)
				return SendMes(playerid, -1, "{FF2222}[Ошибка] {FFFFFF}У банды \"%s\" мало игроков!", GetPlayerFracName[GZInfo[i][gFrak]]);

			if(gFractionFreeze[GZInfo[i][gFrak]-1] > gettime())
				return SendMes(playerid, -1, "{FF2222}[Ошибка] {FFFFFF}Банда \"%s\" заморожена и не может принимать участия в захвате", GetPlayerFracName[GZInfo[i][gFrak]]);

			if(GZInfo[i][gFrak] == faction)
				return SendClientMessage(playerid, COLOR_GREY, !"Вы не можете захватывать зону вашей банды!");

			if(IsCapture != 0)
				return SendClientMessage(playerid, COLOR_WHITE, !"{FF2222}[Ошибка] {FFFFFF}Уже происходит захват одной из зон. Дождитесь окончания!");

            SetPVarInt(playerid, "SelectCapture", typeCapture);
            
			format(stringer, sizeof stringer, "{FFFFFF}Эта территория принадлежит %s\nВы уверены что хотите её захватить?\n\nЧленов вашей банды: {9ACD32}%d\n{FFFFFF}Членов банды \"%s\": {33AAFF}%d\n\n{FFFFFF}Война будет идти по \"{33AAFF}%s{FFFFFF}\"",
				GetPlayerFracName[GZInfo[i][gFrak]], IsPlayerInBandOnline(faction), GetPlayerFracName[GZInfo[i][gFrak]], IsPlayerInBandOnline(GZInfo[i][gFrak]), typeCapture == 1 ? ("убийствам") : ("кол-ву человек"));

			return ShowPlayerDialog(playerid, 838, DIALOG_STYLE_MSGBOX, !"Захват территории", stringer, !"Да", !"Нет");
		}
	}
	return 1;
}
