  		format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ADMIN"` SET `vigs` = '%d' WHERE `admin` = '%d'", AdminInfo[ids][aVigs], PlayerInfo[ids][pID]);
		return mysql_tquery(connects, mysql_query_, "", "");
    }

    format(mysql_query_,sizeof(mysql_query_), "SELECT admin, level, vigs FROM "TABLE_ADMIN" WHERE admin = (SELECT id FROM "TABLE_ACCOUNT" WHERE name = '%s')", name);
	mysql_tquery(connects, mysql_query_, "SetAdmin_Vigs", "dssd", playerid, name, reason, 2);

	return 1;
}
