stock AntiCheat(playerid)
{
	if(gPlayerLogged{playerid} == 0 || IsPlayerNPC(playerid) || !IsPlayerConnected(playerid)) return true;
	if(IsPlayerInVehicle(playerid,GetPlayerVehicleID(playerid)) && (GetPlayerWeapon(playerid) == WEAPON_DEAGLE || GetPlayerWeapon(playerid) == WEAPON_SHOTGSPA || GetPlayerWeapon(playerid) == WEAPON_SAWEDOFF || GetPlayerWeapon(playerid) == WEAPON_MINIGUN || GetPlayerWeapon(playerid) == WEAPON_MP5 )) SetPlayerArmedWeapon(playerid,0);
	iNewPlayerAmmo[playerid] = GetPlayerAmmo(playerid);
	if(iNewPlayerAmmo[playerid] != iPlayerAmmo[playerid])
	{
		OnPlayerAmmoChange(playerid, iNewPlayerAmmo[playerid], iPlayerAmmo[playerid]);
		iPlayerAmmo[playerid] = iNewPlayerAmmo[playerid];
	}
	if(TheifTime[playerid] > 0)
	{
 		if(ProxDetectorS(2.0, playerid, TheifRobID[playerid]))
   		{
			TheifTime[playerid] --;
			if(!TheifTime[playerid])
			{
				if(TheifCount[playerid] < 8)
				{
					SetPlayerRandomKey(playerid, TheifKey[playerid], false);
					ShowPlayerActionBand(playerid, TheifCount[playerid], 8);
				}
			}
		}
		else
		{
  			CheckRobPlayer(playerid);
	    	TogglePlayerControllable(playerid, true);
		}
	}
	if(TheftCarID[playerid] != 0)
	{
		new carid = TheftCarID[playerid];
		new Float:fX,Float:fY,Float:fZ;
		GetVehiclePos(carid, fX, fY, fZ);
		if(IsPlayerInRangeOfPoint(playerid, 2.0, fX, fY, fZ))
		{
		    if(TheftTime[playerid] > 0)
			{
				TheftTime[playerid] --;
				if(TheftTime[playerid] <= 0)
				{
					if(TheftCount[playerid] >= 10)
					{
						TheftKey[playerid] = 0;
						TheftTime[playerid] = 0;
						TheftCount[playerid] = 0;
						TheftCarID[playerid] = 0;
						ToggleLock(carid, 0);
						SendClientMessage(playerid,COLOR_WHITE,"Вы успешно взломали транспорт");
						TogglePlayerControllable(playerid, true);
						HidePlayerActionText(playerid);
						HidePlayerActionBand(playerid);
					}
					else
					{
						SetPlayerRandomKey(playerid, TheftKey[playerid], true);
						ShowPlayerActionBand(playerid, TheftCount[playerid], 10);
					}
				}
			}
		}
		else
		{
			TheftKey[playerid] = 0;
			TheftTime[playerid] = 0;
			TheftCount[playerid] = 0;
			TheftCarID[playerid] = 0;
			FIXES_ApplyAnimation(playerid, "BD_FIRE", "BD_Fire1", 4.1, 0, 0, 0, 0, 1, 1);
			TogglePlayerControllable(playerid, true);
			HidePlayerActionText(playerid);
			HidePlayerActionBand(playerid);
		}
	}
    if(GetPVarInt(playerid,"id_find"))
	{
		if(GetPlayerInterior(GetPVarInt(playerid,"id_find")-1))
		{
			DisablePlayerCheckpoint(playerid);
		    SendClientMessage(playerid, COLOR_WHITE, "Игрок зашел в здание, метка отключена");
		    DeletePVar(playerid,"id_find");
		}
		else
		{
			new Float:X, Float:Y, Float:Z;
			GetPlayerPos(GetPVarInt(playerid,"id_find")-1,X,Y,Z);
			SetPlayerCheckpoint(playerid,X,Y,Z,3);
		}
	}
	return true;
}
publics: GETHP(playerid)
{
    new
	    vehid = GetPlayerVehicleID(playerid);

    if(vehid > 0)
    {
        GetVehicleHealth(vehid, VEHHP[playerid][1]);

        if(VEHHP[playerid][1] >= 1000.0) format(stringer1, sizeof(stringer1), "{ff0000}[ERROR 004/ANTI-GM]: {bbbbbb}%s[%i] использует гм/клео скрипты.", PlayerInfo[playerid][pNames], playerid);
		else format(stringer1, sizeof(stringer1), "{34c924}[ANTI-GM] {bbbbbb}%s[%i] не использует гм/клео скриптты.", PlayerInfo[playerid][pNames],playerid);

		SetVehicleHealth(vehid, VEHHP[playerid][0]);
		return SendAdminMessage(COLOR_LIGHTRED, stringer1);
	}

	GetPlayerHealth(playerid, GMHP[playerid][1]);

	if(GMHP[playerid][1] >= 100.0) format(stringer1, sizeof(stringer1), "{ff0000}[ERROR 003/ANTI-GM]: {bbbbbb}%s[%i] использует гм/клео скрипты.", PlayerInfo[playerid][pNames], playerid);
	else format(stringer1, sizeof(stringer1), "{34c924}[ANTI-GM] {bbbbbb}%s[%i] не использует гм/клео скриптты.", PlayerInfo[playerid][pNames],playerid);

	SetPlayerHealthAC(playerid, GMHP[playerid][0]);
	SetPlayerArmour(playerid, GMARMOUR[playerid][0]);
    return SendAdminMessage(COLOR_LIGHTRED, stringer1);
}
public OnPlayerEnterDynamicCP(playerid, checkpointid)
{
    if(checkpointid == Askyters)
    {
        if(IsPlayerInAnyVehicle(playerid))
			return 1;

    	if(tookfaggio[playerid] != 0) {
	 		DestroyVehicleEx(tookfaggio[playerid]);
	 		tookfaggio[playerid] = 0;
	 		SendClientMessage(playerid, COLOR_RED, "Ваш арендованный мопед, был удален!");
		}
		ShowPlayerDialog(playerid,369,DIALOG_STYLE_MSGBOX,"{0994BA}GOOD | {ffffff}Аренда скутера","{ffffff}Вы действительно хотите арендовать скутер","Да","Нет");
	}
	return 1;
}
public OnVehicleSpawn(vehicleid)
{
	GetVehiclePos(vehicleid, VehicleInfo[vehicleid][vX], VehicleInfo[vehicleid][vY], VehicleInfo[vehicleid][vZ]);
    GetVehicleZAngle(vehicleid, VehicleInfo[vehicleid][vA]);

    if(vehicleid >= derby[0] && vehicleid <= derby[10]) return LinkVehicleToInterior(vehicleid, 15), SetVehicleVirtualWorld(vehicleid, 1);
	foreach(Player,i)
	{
		if(vehicleid == caridhouse[i])
		{
            LinkVehicleToInterior(caridhouse[i], 1);
		    SetVehicleVirtualWorld(caridhouse[i], PlayerInfo[i][pPhousekey]+50);
			AddTune(i, GetPVarInt(i,"CarNomer"));
			break;
		}
		if(tookfaggio[i] == vehicleid && tookfaggio[i] != 0)
		{
	 		DestroyVehicleEx(vehicleid);
	 		tookfaggio[i] = 0;
	 		SendClientMessage(i, COLOR_RED, "Ваш арендованный мопед, был удален!");
		}
	}

	if(LightsObject[vehicleid][0] != -1 || LightsObject[vehicleid][1] != -1
	|| (LightsObject[vehicleid][2] != -1 && LightsObject[vehicleid][3] != -1))
  	{
  	    if(LightsObject[vehicleid][0]!=-1) DestroyDynamicObject(LightsObject[vehicleid][0]), LightsObject[vehicleid][0]=-1;
        if(LightsObject[vehicleid][1]!=-1) DestroyDynamicObject(LightsObject[vehicleid][1]), LightsObject[vehicleid][1]=-1;
        if(LightsObject[vehicleid][2]!=-1) DestroyDynamicObject(LightsObject[vehicleid][2]), LightsObject[vehicleid][2]=-1;
        if(LightsObject[vehicleid][3]!=-1) DestroyDynamicObject(LightsObject[vehicleid][3]), LightsObject[vehicleid][3]=-1;
		SignalTick[vehicleid][0] = 0;
		SignalTick[vehicleid][1] = -1;
		Signal[vehicleid] = 0;
	}
	return 1;
}

publics: OnPlayerRegCheck(playerid, race_check)
{
    if (race_check != g_MysqlRaceCheck[playerid]) return Kick(playerid); // Если сессия несовпадает (запрос выполнился не для того игрока).

	if(cache_num_rows() > 0)
	{
        cache_get_value_name(0, "password", PlayerInfo[playerid][pPassword], 65);
        cache_get_value_name(0, "salt", PlayerInfo[playerid][pSalt], 11);

        PlayerInfo[playerid][Cache_ID] = cache_save();

		ShowPlayerDialog(playerid, 7, DIALOG_STYLE_PASSWORD, !COLOR_TITLE"Авторизация | Пароль", !COLOR_TEXT"Добро пожаловать на сервер "COLOR_NOTE"Good Role Play!\n"COLOR_TEXT"Данный аккаунт зарегистрирован, введите пароль от аккаунта.\nПопыток ввода пароля осталось: "COLOR_ERROR"3 / 3", !"Войти", !"Выйти");
	}
	else
	{
		StartRegister(playerid);
	}
	return true;
}

