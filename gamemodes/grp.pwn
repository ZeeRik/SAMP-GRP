	x += (c_dist * floatsin(-BildInfo[b_id][bPosA], degrees));
    y += (c_dist * floatcos(-BildInfo[b_id][bPosA], degrees));

    SetPlayerCameraPos(playerid, x, y, BildInfo[b_id][bPosZ]);
    SetPlayerCameraLookAt(playerid, BildInfo[b_id][bPosX], BildInfo[b_id][bPosY], BildInfo[b_id][bPosZ]);

	SetPlayerInteriorEx(playerid, 0);
	TogglePlayerControllable(playerid, 0);

	return 1;
}
