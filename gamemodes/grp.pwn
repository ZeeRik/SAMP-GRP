				return SendClientMessage(playerid, -1, !"ID машины не может меньше 400, и больше 611!");

    		else if(params[2] == 537 || params[2] == 538 || params[2] == 569 || params[2] == 570 || params[2] == 590)
				return SendClientMessage(playerid, -1, !"Нельзя указать поезд и прицепы");

			PlayerInfo[params[0]][pCar2] = params[2];
			DeleteVehicleTune(playerid, 1);
		}
		case 3: PlayerInfo[params[0]][pKills] = params[2];
		case 4: PlayerInfo[params[0]][pZakonp] = params[2];
		case 5: PlayerInfo[params[0]][pExp] = params[2];
		case 6: PlayerInfo[params[0]][pMats] = params[2];
		case 7: PlayerInfo[params[0]][pHeal] = params[2];
		case 8: PlayerInfo[params[0]][pCash] = params[2];
		case 9:
		{
			if(params[2] < 1 || params[2] > 50)
				return SendClientMessage(playerid, COLOR_GREY, "Количество билетов от 1 до 50");

			PlayerInfo[params[0]][pRule] = params[2];
			UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `rule` = '%d' WHERE `id` = '%d'", PlayerInfo[playerid][pRule], PlayerInfo[playerid][pID]);
			SendMes(params[0], COLOR_RED, "Основатель %s выдал Вам %d билетов для игры в рулетку (/mm - рулетка)", PlayerInfo[playerid][pNames], params[2]);
			return SendMes(playerid, -1, "Вы выдали %s %d билетов для рулетки", PlayerInfo[params[0]][pNames], params[2]);
		}
		case 10:
		{
			 PlayerInfo[params[0]][pLevel] = params[2];
			 SetPlayerScore(params[0], params[2]);
		     UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `lvl` = '%d' WHERE `id` = '%d'", PlayerInfo[playerid][pLevel], PlayerInfo[playerid][pID]);
		}
		case 11: PlayerInfo[params[0]][Texts][2] = params[2];
		default: return SendClientMessage(playerid, COLOR_GRAD1, "Ошибка: Неверный номер!");
	}
	SendMes(playerid, COLOR_GRAD1, "Слот №%d изменен на %d.", params[1], params[2]);
	OnPlayerUpdateRL(params[0]);

	format(stringer1, sizeof(stringer1), "изменил статистику: [%d] - [%d]", params[1], params[2]);
	Admin_Log(playerid, stringer1, PlayerInfo[params[0]][pNames]);

	format(stringer1, sizeof(stringer1), "< ALog: [%d LVL] > %s (%i) изменил статистику игроку %s (%i): [пункт №%d | значение: %d]", AdminInfo[playerid][aLevel], PlayerInfo[playerid][pNames], playerid, PlayerInfo[params[0]][pNames], params[0], params[1], params[2]);
    return SendAdminMessage(0xFF0000FF, stringer1);
}
