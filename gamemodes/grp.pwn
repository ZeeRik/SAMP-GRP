publics: CheckAdmin(playerid, tmp[], days, reason[], type)
{
	new
		rows;

	cache_get_row_count(rows);

	if(rows)
	{
		new
			adminLevel;

		cache_get_value_name_int(0, "level", adminLevel);

		if(AdminInfo[playerid][aLevel] <= adminLevel)
			return SendClientMessage(playerid, COLOR_REDD, !"Администратор не может быть наказан");
	}

	if(!type)
		mysql_format(connects, mysql_query_, sizeof(mysql_query_), "SELECT 1 FROM "TABLE_BANLOG" WHERE name = '%e'", tmp);
	else if(type == 1)
		format(mysql_query_, sizeof(mysql_query_), "SELECT 1 FROM baniplog WHERE ip = '%s' LIMIT 1", tmp);
	else if(type == 2)
	{
		mysql_format(connects, mysql_query_, sizeof(mysql_query_), "SELECT `id`, `warns` FROM `"TABLE_ACCOUNT"` WHERE `name` = '%e'", tmp);
		return mysql_tquery(connects, mysql_query_, "OffWarn", "dss", playerid, tmp, reason);
	}
	else if(type == 3)
	{
		mysql_format(connects, mysql_query_, sizeof(mysql_query_), "SELECT id, jail FROM "TABLE_ACCOUNT" WHERE name = '%e'", tmp);
		return mysql_tquery(connects, mysql_query_, "OffPrison", "dsds", playerid, tmp, days, reason);
	}
	else if(type == 4)
	{
		mysql_format(connects, mysql_query_, sizeof(mysql_query_), "SELECT id, mute FROM "TABLE_ACCOUNT" WHERE name = '%e'", tmp);
		return mysql_tquery(connects, mysql_query_, "OffMute", "dsds", playerid, tmp, days, reason);
	}

	return mysql_tquery(connects, mysql_query_, "OffBan", "dsdsd", playerid, tmp, days, reason, type);
}
