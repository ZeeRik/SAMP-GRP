CMD:makeadmin(playerid, params[])
{
	if(!AdminInfo[playerid][aLevel])
		return 1;

	format(mysql_query_, sizeof(mysql_query_), "SELECT 1 FROM "TABLE_ADMIN" WHERE admin = %d AND full_dostup = 1", PlayerInfo[playerid][pID]);
	new Cache:result = mysql_query(connects, mysql_query_);

	new rows = cache_num_rows();
	cache_delete(result);
	if(!rows) return 1;

	extract params -> new player:a_id, lvl; else
		return SendClientMessage(playerid, -1, !"Введите: /makeadmin [ID] [уровень]");

  	if(!IsPlayerConnected(a_id) || !PlayerInfo[a_id][pID])
		return SendClientMessage(playerid, COLOR_WHITE, !"Ошибка: Игрок не найден.");

  	else if(!(0 <= lvl < 9))
  		return SendClientMessage(playerid, 0xAFAFAFAA, !"Неверный уровень. Значение от 0 до 8.");

   	else if((PlayerInfo[a_id][pID] == 1 || PlayerInfo[a_id][pID] == 2) && PlayerInfo[playerid][pID] != 1 && PlayerInfo[playerid][pID] != 2)
   		return SendClientMessage(playerid, 0xAFAFAFAA, !"Ошибка прав доступа!");

    if(!lvl) {

    	if(!AdminInfo[a_id][aLevel])
    		return SendClientMessage(playerid, -1, !"Указанный игрок не является администратором.");

      	SendMes(a_id, COLOR_ORANGE, "Главный администратор %s (%i) аннулировал Вам полномочия администратора!", PlayerInfo[playerid][pNames], playerid);
       	SendMes(playerid, COLOR_ORANGE, "Вы аннулировали права администратора у %s (%i)", PlayerInfo[a_id][pNames], a_id);

		DelAdmin(a_id);
  		Admin_Log(playerid, "снял с поста администратора", PlayerInfo[a_id][pNames]);
	}

 	else if(!AdminInfo[a_id][aLevel]) {

		SendMes(a_id, COLOR_ORANGE, "Главный администратор %s [%i] назначил Вас на ранг %s (%d уровень)", PlayerInfo[playerid][pNames], playerid, AdminRank[lvl][aRank], lvl);
		SendClientMessage(a_id, COLOR_ORANGE, !"Для регистрации пароля используйте команду /alogin. Помощь по командам /ahelp.");

		AdminInfo[a_id][aLevel] = lvl;

		format(mysql_query_, sizeof(mysql_query_), "INSERT INTO `"TABLE_ADMIN"` ( `admin`, `level` ) VALUES ( '%d', '%d')", PlayerInfo[a_id][pID], lvl);
  		mysql_tquery(connects, mysql_query_, "", "" );

   		SendMes(playerid, COLOR_ORANGE, "Вы назначили %s [%i] на ранг %s (%d уровень)", PlayerInfo[a_id][pNames], a_id, AdminRank[lvl][aRank], lvl);

   		format(stringer1, sizeof(stringer1), "назначил на пост администратора, уровень %d", lvl);
   		Admin_Log(playerid, stringer1, PlayerInfo[a_id][pNames]);
	}

	else
	{
 		AdminInfo[a_id][aLevel] = lvl;

   		SendMes(a_id, COLOR_ORANGE, "Главный администратор %s [%i] изменил Ваш ранг администрирование на %s (%i).", PlayerInfo[playerid][pNames], playerid, AdminRank[lvl][aRank], lvl);
     	SendMes(playerid, COLOR_ORANGE, "Вы изменили ранг администрирование %s [%i] на %s (%i).", PlayerInfo[a_id][pNames], a_id, AdminRank[lvl][aRank], lvl);

      	format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ADMIN"` SET `level` = '%d' WHERE `admin` = '%d'", lvl, PlayerInfo[a_id][pID]);
		mysql_tquery(connects, mysql_query_, "", "");
	}

	return 1;
}
