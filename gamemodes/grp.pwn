CMD:recon(playerid,params[])
{
    if(!AdminInfo[playerid][aLogin])
	    return 1;

    extract params -> new player:targetid; else
        return SendClientMessage(playerid, -1, !"Введите: /re(con) [ID игрока]");

    if(!IsPlayerConnected(targetid) || targetid == playerid)
	    return SendClientMessage(playerid,COLOR_GRAD1, !"Ошибка: Игрока не существует или это Вы");
	    
	new PlayerState = GetPlayerState(targetid);

	if(PlayerState == PLAYER_STATE_NONE || PlayerState == PLAYER_STATE_WASTED || PlayerState == PLAYER_STATE_SPECTATING)
		return SendClientMessage(playerid, 0xBB0000FF, "Ошибка: Игрок невступил в игру");

	if(AdminInfo[targetid][aLevel] > AdminInfo[playerid][aLevel] || PlayerInfo[targetid][pID] == 1)
		return SendClientMessage(playerid,COLOR_GRAD1, "Наблюдение за администрацией запрещено!");

    if(spectateID[playerid] == INVALID_PLAYER_ID)
    {
	    oldPlayerVirtualWorld[playerid] = GetPlayerVirtualWorld(playerid);
		oldPlayerInterior[playerid] = GetPlayerInterior(playerid);
		GetPlayerPos(playerid, oldPlayerPosition[playerid][0], oldPlayerPosition[playerid][1], oldPlayerPosition[playerid][2]);
		GetPlayerFacingAngle(playerid, oldPlayerPosition[playerid][3]);
		GetPlayerHealth(playerid, oldPlayerHealth[playerid]);
		GetPlayerArmour(playerid, oldPlayerArmour[playerid]);
		
		SendMes(playerid, 0xFFA500FF, "[Информация] {FFFFFF}Вы начали слежку за игроком {FFA500}%s{FFFFFF}[{FFA500}%d{FFFFFF}]", PlayerInfo[targetid][pNames], targetid);
		SendClientMessage(playerid, 0xFFA500FF, !"[Информация] {FFFFFF}Чтобы включить режим камеры используйте {FFA500}/camera {FFFFFF}или клавишу {FFA500}ESC");
	}
	else isPlayerSpectate[spectateID[playerid]] --;
	
	isPlayerSpectate[targetid] ++;
    return StartSpectate(playerid, targetid);
}
