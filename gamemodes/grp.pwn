stock GetNearestVehicle(playerid)
{
    new Float:X,
		Float:Y,
		Float:Z;
		
	for (new vehid = GetVehiclePoolSize(); vehid != 0; --vehid)
	{
	    if(IsVehicleStreamedIn(vehid, playerid) == 0)
	    	continue;
	    	
		GetVehiclePos(vehid, X, Y, Z);

		if(IsPlayerInRangeOfPoint(playerid, 4.0, X, Y, Z))
			return vehid;
	}
	
	return -1;
}
