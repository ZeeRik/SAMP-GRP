CMD:prison(playerid, params[])
{
	if(AdminInfo[playerid][aLevel] < 1 || !AdminInfo[playerid][aLogin])
		return 1;

	extract params -> new player:targetid, time, string:reason[65] = "причина не указана"; else
        return SendClientMessage(playerid, -1, !"Введите: /prison [ID] [минуты (0 - 60), 0 = освободить] [причина]");

	if(!IsPlayerConnected(targetid) || targetid == playerid)
		return SendClientMessage(playerid, COLOR_GRAD1, !"Ошибка: Игрока не существует или это Вы");

	if(IsIpFound(reason) || CheckString(reason))
		return DelAdmin(playerid, 1);

	if((AdminInfo[targetid][aLevel] > AdminInfo[playerid][aLevel]))
		return SendClientMessage(playerid, COLOR_GRAD2, !"Ошибка: игрок не может быть наказан");

	if (time < 0 || time > 60)
		return SendClientMessage(playerid, COLOR_GRAD1, "Ошибка: время должно быть от 0 - 60");

	if(!time)
 	{
 	    if(PlayerInfo[targetid][pJailed] != 4)
 			return SendClientMessage(playerid, COLOR_WHITE, "Игрок не в тюрьме!");

		SendMes(targetid, COLOR_LIGHTRED, "Вы были освобождены администратором %s", PlayerInfo[playerid][pNames]);

		format(stringer1, sizeof(stringer1), "[A] %s освободил игрока %s из деморгана. Причина: %s", PlayerInfo[playerid][pNames],PlayerInfo[targetid][pNames], reason);
		SendAdminMessage(COLOR_LIGHTRED,stringer1);

  		PlayerInfo[targetid][pJailed] = PlayerInfo[targetid][pJailTime] = 0;
  		SpawnPlayerEx(targetid);
 	}
 	else
 	{
		ResetWeapons(targetid);

		SetPlayerInteriorEx(targetid, 0);
		SetPlayerPos(targetid,2543.0537,-8141.8242,72.6420);
		SetPlayerFacingAngle(targetid, 268.7242);

		PlayerInfo[targetid][pJailed] = 4;
		PlayerInfo[targetid][pMestoJail] = 0;
		PlayerInfo[targetid][pJailTime] = time * 60;
		SendMes(targetid, COLOR_LIGHTRED, "Администратор %s посадил вас в деморган на %d минут(ы). Причина: %s", PlayerInfo[playerid][pNames], time, reason);

		format(stringer1, sizeof(stringer1), "[A] %s посадил игрока %s в деморган на %d минут(ы). Причина: %s", PlayerInfo[playerid][pNames],PlayerInfo[targetid][pNames], time, reason);
		SendAdminMessage(COLOR_LIGHTRED,stringer1);
	}

	PlayerInfo[targetid][pWanted] = 0;
	UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `jail` = '%d', `jail_time` = '%d', `jail_place` = '%d', `wanted` = '%d' WHERE `id` = '%d'", PlayerInfo[targetid][pJailed], PlayerInfo[targetid][pJailTime], PlayerInfo[targetid][pMestoJail], PlayerInfo[targetid][pWanted], PlayerInfo[targetid][pID]);

	return 1;
}
CMD:giverank(playerid, params[])
{
    if(!PlayerInfo[playerid][pMember])
		return 1;

	if(PlayerInfo[playerid][pRank] < gFractionRank[PlayerInfo[playerid][pMember]-1][2])
	 	return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PlayerInfo[playerid][pMember]-1][2]);

    extract params -> new player:targetid, rank; else
        return SendClientMessage(playerid, -1, !"Введите: /giverank [ID] [ранг]");

    if(!IsPlayerConnected(targetid) || targetid == playerid)
		return SendClientMessage(playerid, COLOR_GRAD1, !"Ошибка: Игрока не существует или это Вы");

    if(PlayerInfo[targetid][pMember] != PlayerInfo[playerid][pMember])
        return SendClientMessage(playerid, COLOR_GRAD1, !"Ошибка: Игрок не в Вашей организации");

	if(!PlayerInfo[playerid][pLeader] && (PlayerInfo[targetid][pRank] >= gFractionRank[PlayerInfo[playerid][pMember]-1][2]))
		return SendClientMessage(playerid, COLOR_GRAD1, !"Ошибка: Вы не можете изменить ранг этому игроку");

	switch(PlayerInfo[playerid][pMember])
    {
   	    case 1,10,3,19,21: if(rank > 14 || rank < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 14!");
        case 2,26,4,5,6,11,12,13,14,15,17,18,22,24,25,7: if(rank > 9 || rank < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 9!");
        case 27,28: if(rank > 4 || rank < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 4!");
        case 16: if(rank > 12 || rank < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 12!");
        case 23: if(rank > 11 || rank < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 11!");
    }

    switch(PlayerInfo[playerid][pMember])
	{
	    case 1..4, 7, 10, 11, 19, 21, 25, 26:
		{
	        if(PlayerInfo[playerid][pOdet] == 0)
				return SendClientMessage(playerid,COLOR_WHITE, !"Вы не на службе!");

	        if(PlayerInfo[targetid][pRank] <= 5 && rank > 5)
	        {
	            if(PlayerInfo[targetid][pMedCard] == 0)
					return SendClientMessage(playerid, COLOR_WHITE,"Для получения данного ранга, у игрока должна быть медицинская карта.");

				if((PlayerInfo[targetid][pMember] == 1
					|| PlayerInfo[targetid][pMember] == 2
					|| PlayerInfo[targetid][pMember] == 10
					|| PlayerInfo[targetid][pMember] == 21
					|| PlayerInfo[targetid][pMember] == 25)
					&& PlayerInfo[targetid][pVoenBill] == 0)
	            {
	                return SendClientMessage(playerid,COLOR_WHITE,"Для получения данного ранга, у игрока должен быть военный билет.");
	            }
	        }
		}
	}

    if((PlayerInfo[targetid][pMember] == 3 || PlayerInfo[targetid][pMember] == 19)
		&& rank >= 3
		&& PlayerInfo[targetid][pVoenBill] == 0)
	{
	    PlayerInfo[targetid][pVoenBill] = 1;

		SendClientMessage(targetid, COLOR_GRAD1, "Поздравляем! Вы получили военный билет!");

	    format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `voenbill` = '1' WHERE `id` = '%d'",PlayerInfo[targetid][pID]);
	    mysql_tquery(connects, mysql_query_, "", "");
	}

	if(!PlayerInfo[playerid][pLeader] && (PlayerInfo[playerid][pRank] <= PlayerInfo[targetid][pRank]))
		return SendClientMessage(playerid, COLOR_GREY, "Вы не можете установить такой ранг");

	SendMes(targetid, 0x6BB3FFAA, "Вас %sи до %d ранга", PlayerInfo[targetid][pRank] > rank ? ("понизил") : ("повысил"), rank);

	format(stringer1, sizeof(stringer1), "[F] Сотрудник %s %s игрока %s до %d ранга", PlayerInfo[playerid][pNames], PlayerInfo[targetid][pRank] > rank ? ("понизил") : ("повысил"), PlayerInfo[targetid][pNames], rank);
	SendFamilyMessage(PlayerInfo[playerid][pMember], 0x4682B4AA, stringer1);

    PlayerInfo[targetid][pRank] = rank;
    SetPlayerStraps(targetid);

    UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `rank` = '%d' WHERE `id` = '%d'", PlayerInfo[targetid][pRank], PlayerInfo[targetid][pID]);

	return 1;
}
