stock IsVehicleOccupied(vehicleid)
{
	foreach(new i : Player)
		if(IsPlayerInVehicle(i, vehicleid)) return 1;

	return 0;
}

stock GetIPSubnet(buffer[])
{
    for(new i, dots; ; ++i)
        switch(buffer[i])
        {
            case '\0':
                break;
            case '.':
                if(++dots == 2)
                {
                    buffer[i] = '\0';
                    break;
                }
        }
}
stock SetNewName(playerid, name[])
{
    if(SetPlayerName(playerid, name) != 1)
        return SendMes(playerid, COLOR_GREY, "Ошибка: никнейм %s не может быть применен!", name);
    
    format(stringer1, sizeof(stringer1), "<< %s сменил никнейм на: %s >>", PlayerInfo[playerid][pNames], name);
	SendClientMessageToAll(COLOR_LIGHTRED, stringer1);

	new housekey = PlayerInfo[playerid][pPhousekey];
    if(housekey != 255)
    {
    	strmid(HouseInfo[housekey][hOwner], name, 0, strlen(name), MAX_PLAYER_NAME + 1);
        SaveHouse(housekey);
	}
    if(GetPlayerBizz(playerid))
    {
    	new i = GetPVarInt(playerid, "PlayerBizz");
        strmid(BizzInfo[i][bOwner],name,0,strlen(name), MAX_PLAYER_NAME + 1);
        SaveBizz(i);
        UpdateBizz(i);
	}
	if(PlayerInfo[playerid][pFamily] && !strcmp(PlayerInfo[playerid][pNames], FamilyInfo[playerid][fLeader], true))
	{
	    strmid(FamilyInfo[playerid][fLeader], name, 0, strlen(name), MAX_PLAYER_NAME + 1);
	    
	    mysql_format(connects, mysql_query_, sizeof(mysql_query_), "UPDATE family SET leader = '%e' WHERE id = %d", FamilyInfo[playerid][fLeader], PlayerInfo[playerid][pFamily]);
		mysql_tquery(connects, mysql_query_, "", "" );
	}
	
	mysql_format(connects, mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `name` = '%e' WHERE `id` = '%d'", name, PlayerInfo[playerid][pID]);
	mysql_tquery(connects, mysql_query_, "", "");

	mysql_format(connects, mysql_query_, sizeof(mysql_query_), "INSERT INTO `setname` ( `account`, `old_nick`, `new_nick` ) VALUES ( '%d', '%s', '%e')", PlayerInfo[playerid][pID], PlayerInfo[playerid][pNames], name);
	mysql_tquery(connects, mysql_query_, "", "" );
	
	GetPlayerName(playerid, PlayerInfo[playerid][pNames], MAX_PLAYER_NAME + 1);
	
	return 1;
}
