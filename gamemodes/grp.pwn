						SetPlayerPos(i,-1607.1873,721.3649,12.2721);//Тюрьма
						SetPlayerFacingAngle(i, 2.3026);
					}
					if(PlayerInfo[i][pMestoJail] == 3)
					{
						SetPlayerPos(i,2334.8467,2454.9456,14.9688);//Тюрьма
						SetPlayerFacingAngle(i, 115.7874);
					}
				}
				else if(PlayerInfo[i][pJailed] == 4)
				{
					SetPlayerPos(i,1547.11,-1675.76,13.94);//Тюрьма
					SetPlayerFacingAngle(i, 95.0636);
				}
				SetPlayerInteriorEx(i, 0);
				PlayerInfo[i][pJailTime] = 0;
				PlayerInfo[i][pJailed] = 0;
                SendClientMessage(i, COLOR_ISPOLZUY,"Вы заплатили свой долг обществу, теперь вы свободны!");
				PlayerInfo[i][pMestoJail] = 0;
				PlayerInfo[i][pWanted] = 0;
				SetPlayerWantedLevel(i, 0);
				SetPlayerVirtualWorldEx(i, 0);
				GameTextForPlayer(i, "~g~Freedom", 5000, 1);
				SetPlayerToTeamColor(i, PlayerInfo[i][pMember]);
				UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `jail` = '0', `jail_time` = '0', `jail_place` = '0', `wanted` = '0' WHERE `id` = '%d'", PlayerInfo[i][pID]);
			}
		}
		if(JobCarTime[i] <= 16 && JobCarTime[i] >= 1)
		{
			JobCarTime[i]--;
			format(stringer1,sizeof(stringer1),"~r~%d",JobCarTime[i] - 1);
			GameTextForPlayer(i, stringer1, 2000, 6);
		}
		if(Tazer[i] == 1)
		{
			if(TazerTime[i] <= 0) Tazer[i] = 0;
			else TazerTime[i] -= 1;
		}
		if(TieTime[i] > 0)
		{
			TieTime[i]--;
			if(TieTime[i] == 0) gag[i] = 0,SendClientMessage(i, COLOR_GREY, "Кляп стал мокрым и выпал, вы снова можете говорить!");
		}
		if(PlayerCuffed[i] == 1)
		{
			if(PlayerCuffedTime[i] <= 0)
			{
				TogglePlayerControllable(i, 1);
				PlayerCuffed[i] = 0;
				PlayerCuffedTime[i] = 0;
			}
			else PlayerCuffedTime[i] -= 1;
		}
		if(PlayerCuffed[i] == 2)
		{
			if(PlayerCuffedTime[i] <= 0)
			{
				TogglePlayerControllable(i, 1);
				PlayerCuffed[i] = 0;
				PlayerCuffedTime[i] = 0;
			}
			else PlayerCuffedTime[i] -= 1;
		}
		if(PlayerInfo[i][pHospital]) {

			PlayerInfo[i][pHealTime] ++;
			if(PlayerInfo[i][pHealTime] >= (5 + random(5)))
			{
				if(PlayerBed[i] != -1)
				{
					PlayerHealth[i] += 6;
					GameTextForPlayer(i, "+6 HP", 2000, 4);
				}
				else
				{
					PlayerHealth[i] += 3;
					GameTextForPlayer(i, "+3 HP", 2000, 4);
				}

				if(PlayerHealth[i] >= 100.0)
				{
					if(PlayerBed[i] != -1)
					{
						TogglePlayerControllable(i, true);
						SetPlayerPos(i, HealInfo[PlayerBed[i]][hCP][0], HealInfo[PlayerBed[i]][hCP][1], HealInfo[PlayerBed[i]][hCP][2]);
						CheckBed(i);
					}
					PlayerInfo[i][pHospital] = 0;
					PlayerHealth[i] = 100.0;
					SendClientMessage(i, COLOR_GREEN, !"Курс лечения завершен. Теперь вы можете выйти из больницы");
				}
				SetPlayerHealthAC(i, PlayerHealth[i]);
				PlayerInfo[i][pHealTime] = 0;
			}
		}
	}
	if ( minute == 15 && second == 0 || minute == 30 && second == 0 || minute == 40 && second == 0 || minute == 55  && second == 0 )
	{
	    switch ( random ( 5 ) )
	    {
	        case 0:
	        {
	        	SendClientMessageToAll(0x4582A1FF, "[Информация] {ffffff}На сервере запрещены ДМ, ДБ, Флуд, любого рода читы, оскорбления и т.д. Команды сервера /mm.");
				SendClientMessageToAll(0x4582A1FF, "[Информация] {ffffff}Увидели читера? Используйте /(rep)ort [ID] [жалоба] и его накажут.");
	        }
	        case 1:
	        {
	            SendClientMessageToAll(0x4582A1FF, "[Информация] {ffffff}У Вас возникли вопросы по игре? Не знаете что делать? Спросите у Нас и мы ответим /ask [вопрос]");
				SendClientMessageToAll(0x4582A1FF, "[Информация] {ffffff}Не знаете куда идти? Система /gps всегда поможет Вам.");
	        }
	        case 2:
			{
			    SendClientMessageToAll(0x4582A1FF, "[Информация] {ffffff}Хотите приобрести дом, машину или вступить в организацию? Подайте объявление. (/ad текст)");
				SendClientMessageToAll(0x4582A1FF, "[Информация] {ffffff}Подать объявление можно в любом месте, главное иметь телефон.");
			}
			case 3:
			{
			    SendClientMessageToAll(0x4582A1FF, "[Информация] {ffffff}Каждый час на сервере проводятся наборы в организации и в лидеров организаций");
				SendClientMessageToAll(0x4582A1FF, "[Информация] {ffffff}А также проходят интересные мероприятия и незабываемые конкурсы. Оставайтесь с нами!");
			}
			case 4:
			{
			    SendClientMessageToAll(0x4582A1FF, "[Информация] {ffffff}Не забывайте посещать нашу группу во ВКонтакте: vk.com/grp.game");
				SendClientMessageToAll(0x4582A1FF, "[Информация] {ffffff}Там Вы сможете узнать всю самую актуальную информацию о сервере и принять участия в конкурсах");
			}
	    }
	}
	if(hour == 00 || hour == 03 || hour == 06 || hour == 09 || hour == 12 || hour == 15 || hour == 18 || hour == 21)
	{
	    if(hour == 00 && minute == 00 && second == 00)
		{
	    	foreach (new i : Player) {
				if(gPlayerLogged{i}) {
					SaveOnline(i);
					PlayerInfo[i][online_player] = PlayerInfo[i][afk_online] = 0;
				}
			}
		}
		if(hour == 06 && minute == 00 && second == 00)
		{
			mysql_tquery(connects, "UPDATE `"TABLE_ACCOUNT"` SET `helper_asks` = '0'", "", "");
			foreach (new i : Player) PlayerInfo[i][pHotvet] = 0;

			for(new x; x < MAX_BILLBORDS; x++) {
				if(strlen(BildInfo[x][bOwner]) >= 4) {
				    BildInfo[x][bDay] -= 1;

				    if(BildInfo[x][bDay] <= 0) {
				        ClearBillBord(x);
				    }
				    else UpdatePlayerData("UPDATE "TABLE_BILLBOARD" SET day = %d WHERE id = %d", BildInfo[x][bDay], BildInfo[x][bID]);
				}
			}
		}
		if(minute == 32 && second == 00)
	    {
			for(new h = 0; h < sizeof(HouseInfo); h++)
			{
				if(HouseInfo[h][hKlass] == 0 && HouseInfo[h][hOwned] == 1)//Классы домов
				{
					HouseInfo[h][hTakings] -=300; kazna += 300;
					if(HouseInfo[h][hTakings] > 16800) HouseInfo[h][hTakings] = 16800;
				}
				if(HouseInfo[h][hKlass] == 1 && HouseInfo[h][hOwned] == 1)//Классы домов
				{
					HouseInfo[h][hTakings] -=600; kazna += 600;
					if(HouseInfo[h][hTakings] > 33600) HouseInfo[h][hTakings] = 33600;
				}
				if(HouseInfo[h][hKlass] == 2 && HouseInfo[h][hOwned] == 1)//Классы домов
				{
					HouseInfo[h][hTakings] -=900; kazna += 900;
					if(HouseInfo[h][hTakings] > 50400) HouseInfo[h][hTakings] = 50400;
				}
				if(HouseInfo[h][hKlass] == 3 && HouseInfo[h][hOwned] == 1)//Классы домов
				{
					HouseInfo[h][hTakings] -=1200; kazna += 1200;
					if(HouseInfo[h][hTakings] > 67200) HouseInfo[h][hTakings] = 67200;
				}
				if(HouseInfo[h][hKlass] == 4 && HouseInfo[h][hOwned] == 1)//Классы домов
				{
					HouseInfo[h][hTakings] -=1500; kazna += 1500;
					if(HouseInfo[h][hTakings] > 84000) HouseInfo[h][hTakings] = 84000;
				}
				if(HouseInfo[h][hTakings] <= 0 && HouseInfo[h][hOwned] == 1)
				{
					new housecash = HouseInfo[h][hValue];
					new playeridd = GetPlayerID(HouseInfo[h][hOwner]);
					if(IsPlayerConnected(playeridd))
					{
						PlayerInfo[playeridd][pPhousekey] = 255;
						if(caridhouse[playeridd] != INVALID_VEHICLE_ID)
						{
                            DellNeon(caridhouse[playeridd]);
							DestroyVehicleEx(caridhouse[playeridd]);
							caridhouse[playeridd] = INVALID_VEHICLE_ID;
							DeletePVar(playeridd, "CarNomer");
						}
						SendClientMessage(playeridd, COLOR_LIGHTRED, "Ваш дом был продан за неуплату государтсву.");
					}
					else if(!IsPlayerConnected(playeridd))
					{
						UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `house` = '255', `house_cash` = '%d' WHERE `name` = '%s'", housecash, HouseInfo[h][hOwner]);
					}
					ClearHouse(h);
				}
				HouseInfo[h][hRobhouse] = false;
			}
	    }
	}
	if(IsCapture != 0) GzCheck();
	if(BizzWars == 0 && BizzWarsNumber == -1) return true;
	if(BizzWarTime > 0)
	{
		BizzWarTime --;
		CheckBizWar();
	}
	else FinishBizWar();
	return true;
}
