		return SendClientMessage(playerid, COLOR_GREY, !"Игрок не находится в какой либо организации");

    if(IsPlayerStartCroupier(targetid))
    {
        if(CroupierTable[targetid] != -1)
	    {
	        switch(CroupierGame[targetid])
	        {
