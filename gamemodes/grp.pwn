stock FinishBizWar()
{
    new PlayerNapad = 0;
    new PlayerGuard = 0;
    foreach (new i : Player)
	{
	    if(gPlayerLogged{i} == 0) continue;
	    if(BizzWar[bwNapad] != PlayerInfo[i][pMember] && BizzWar[bwGuard] != PlayerInfo[i][pMember]) continue;
        if(PlayerToKvadrat(i,gBizzWarZone[0], gBizzWarZone[1], gBizzWarZone[2], gBizzWarZone[3]))
		{
		    if(BizzWar[bwNapad] == PlayerInfo[i][pMember]) PlayerNapad++;
		    if(BizzWar[bwGuard] == PlayerInfo[i][pMember]) PlayerGuard++;
		}
	}
	if(BizzWars == 1 && (GangInfo[BizzWar[bwNapad]][score] <= GangInfo[BizzWar[bwGuard]][score])
		|| BizzWars == 2 && ((PlayerGuard > 0 && PlayerNapad == 0) || (PlayerGuard == 0 && PlayerNapad == 0)))
	{
	    format(stringer1, sizeof(stringer1), "Неудача. Вам не удалось захватить бизнес %s",BizzInfo[BizzWarsNumber][bName]);
		SendFamilyMessage(BizzWar[bwNapad], COLOR_LIGHTRED,stringer1);
		format(stringer1, sizeof(stringer1), "Вам удалось выстоять войну за бизнес %s",BizzInfo[BizzWarsNumber][bName]);
		SendFamilyMessage(BizzWar[bwGuard], COLOR_GREEN,stringer1);
		ClearBizWar();
	}
	else if(BizzWars == 1 && (GangInfo[BizzWar[bwNapad]][score] > GangInfo[BizzWar[bwGuard]][score] ||
			GangInfo[BizzWar[bwNapad]][score] == 0 && GangInfo[BizzWar[bwGuard]][score] == 0)
			|| BizzWars == 2 && (PlayerNapad > 0 && PlayerGuard == 0))
	{
		format(stringer1, sizeof(stringer1), "Теперь бизнес %s под контролем вашей мафии.",BizzInfo[BizzWarsNumber][bName]);
		SendFamilyMessage(BizzWar[bwNapad], COLOR_GREEN,stringer1);
		SendFamilyMessage(BizzWar[bwNapad], COLOR_GREEN,"Каждый час вы будете получать 15 процентов от заработка бизнеса.");
