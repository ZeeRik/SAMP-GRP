publics: OnLeaderPanel(playerid, page)
{
	new rows;
    cache_get_row_count(rows);
    
	if(!rows)
		return SendClientMessage(playerid,-1,"• {AC0000}[Ошибка] {ffffff}Информация о сотрудниках не найдена.");
		
	page_identifier[playerid][0] = page_number[playerid] = 0;
	
	new
	    name[MAX_PLAYER_NAME + 1],
		account_id,
		rank,
		last_visit[32];
		
	format(stringer, sizeof(stringer),"{FFFFFF}Имя_Фамилия\t{FFFFFF}Ранг\t{FFFFFF}Последний вход\n{FF6F00}Перейти на следующую страницу\nПерейти на предыдущую страницу\n");

	for(new i; i < rows; i++)
	{
		cache_get_value_name_int(i, "id", account_id);
		cache_get_value_name(i, "name", name, MAX_PLAYER_NAME);
		cache_get_value_name_int(i, "rank", rank);
		cache_get_value_name(i, "geton_date", last_visit, 32);
		
		page_identifier[playerid][page_number[playerid]] = account_id;
		page_number[playerid] ++;
		
		format(stringer, sizeof(stringer), "%s%d. %s\t[{FF6F00}%s (%d){FFFFFF}]\t[{FF6F00}%s{FFFFFF}]\n",
			stringer, i + 1, name, fRankName[PlayerInfo[playerid][pMember] - 1][rank - 1],
			rank, last_visit);
	}

	SetPVarInt(playerid, #page_str, page);

	format(stringer1, sizeof(stringer1), "{FF6F00}Список OFFline игроков страница №%d", page);
	return ShowPlayerDialog(playerid, 758, DIALOG_STYLE_TABLIST_HEADERS, stringer1, stringer, "Принять", "Отмена");
}
publics: OFFStats(playerid)
{
	new rows;
    cache_get_row_count(rows);
	if(!rows) return 1;
	new lastvhod[32], nicks [20], rank, vigs;
    cache_get_value_name_int(0, "rank", rank);
	if(!rank) return 1;
	cache_get_value_name_int(0, "reprimands", vigs);
	cache_get_value_name(0, "name", nicks, 20);
	cache_get_value_name(0, "geton_date", lastvhod, 32);
	format(stringer, sizeof(stringer), "{FFFFFF}Имя_Фамилия: {FF6F00}%s\n{FFFFFF}Ранг: {FF6F00}%s (%d)\n{FFFFFF}Выговоров: {FF6F00}%d / 3\n{FFFFFF}Был в сети: {FF6F00}%s{FFFFFF}\n- Уволить\n- Повысить\n- Понизить\n- Выдать выговор\n- Снять выговор", nicks, fRankName[PlayerInfo[playerid][pMember]-1][rank-1], rank, vigs, lastvhod);
	return ShowPlayerDialog(playerid, 759, DIALOG_STYLE_LIST, "{FF6F00}Информация о игроке", stringer, "Выбрать", "Отмена");
}
publics: OFFLP_DATA(playerid, ids, act)
{
	new rows;
    cache_get_row_count(rows);
	if(!rows) return 1;
	new nicks[20], rank, vigs, member;
	cache_get_value_name_int(0, "rank", rank);
	cache_get_value_name_int(0, "reprimands", vigs);
	cache_get_value_name(0, "name", nicks, 20);
	cache_get_value_name_int(0, "member", member);
	if(IsPlayerConnected(GetPlayerID(nicks))) return SendClientMessage(playerid,-1,"Данный игрок в сети.");
	if(act == 4) {
        SendMes(playerid, -1, "Вы уволили игрока %s.",nicks);
        if(rank >= 3) // трудовая
		{
            mysql_format(connects, mysql_query_, sizeof(mysql_query_), "UPDATE `employment_history` SET `date_uninvite` = NOW(), `rang` = '%d', `frac_name` = '%e', `position` = '%e' WHERE `id` = '%d'",
	        rank, GetPlayerFracName[member], fRankName[member-1][rank-1], ids);
		}
		else
		{
		    format(mysql_query_,sizeof(mysql_query_),"DELETE FROM `employment_history` WHERE `account` = '%d' AND `date_uninvite` IS NULL AND `rang` IS NULL AND `frac_name` = 'unknown_frac' AND `position` = 'unknown_position'", ids);
		}
        mysql_tquery(connects, mysql_query_, "", "");

		format(mysql_query_,sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `member` = '0', `rank` = '0', `unit` = '0' WHERE `id` = '%d'", ids);
	}
    else if(act == 5) {
        switch(PlayerInfo[playerid][pMember])
        {
   		    case 1,10,3,19,21: if(rank >= 14) return SendClientMessage(playerid, COLOR_GREY, "Ранг не может быть больше 14. Действие отменено.");
            case 2,26,4,5,6,11,12,13,14,15,17,18,22,24,25,7: if(rank >= 9) return SendClientMessage(playerid, COLOR_GREY, "Ранг не может быть больше 9. Действие отменено.");
            case 27,28: if(rank >= 4) return SendClientMessage(playerid, COLOR_GREY, "Ранг не может быть больше 4. Действие отменено.");
            case 16: if(rank >= 12) return SendClientMessage(playerid, COLOR_GREY, "Ранг не может быть больше 12. Действие отменено.");
            case 23: if(rank >= 11) return SendClientMessage(playerid, COLOR_GREY, "Ранг не может быть больше 11. Действие отменено.");
        }
        SendMes(playerid, -1, "Вы повысили игрока %s до %d ранга.",nicks, rank+1);
		format(mysql_query_,sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `rank` = '%d' WHERE `id` = '%d'", rank+1,ids);
	}
	else if(act == 6) {
		if(rank == 1) return SendClientMessage(playerid, COLOR_GREY, "У игрока первый ранг. Понизить невозможно.");
        SendMes(playerid, -1, "Вы понизили игрока %s до %d ранга.",nicks, rank-1);
		format(mysql_query_,sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `rank` = '%d' WHERE `id` = '%d'", rank-1,ids);
	}
	else if(act == 7) {
		vigs ++;
		if(vigs >= 3) {
            SendMes(playerid, -1, "Вы уволили игрока %s (3 / 3).",nicks);
			format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `leader` = '0', `member` = '0', `rank` = '0', `reprimands` = '0', `duty` = '0', `unit` = '0' WHERE `id` = '%d'", ids);
        }
		else {
            SendMes(playerid, -1, "Вы выдали игроку %s выговор (%d / 3).",nicks, vigs);
			format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `reprimands` = '%d' WHERE `id` = '%d'", vigs,ids);
		}
	}
	else if(act == 8) {
		if(vigs == 0) return SendClientMessage(playerid, COLOR_GREY, "У игрока отсутствуют выговоры.");
        SendMes(playerid, -1, "Вы сняли выговор у игрока %s (%d / 3).",nicks, vigs-1);
		format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `reprimands` = '%d' WHERE `id` = '%d'", vigs-1,ids);
	}
	mysql_tquery(connects, mysql_query_, "", "");
	return 1;
}
publics: OnFracRankLoad()
{
	new
		rows,
		fraction,
		load_data[20];

	cache_get_row_count(rows);
	if(!rows) return print("Ранги не загружены.");

	for(new r = 0; r < rows; r++)
	{
	    cache_get_value_index_int(r, 0, fraction);
	    for(new i = 1; i < 16; i++) cache_get_value_index(r, i, fRankName[fraction][i-1], 40);

	    cache_get_value_index(r, 16, load_data, 20);
	    sscanf(load_data,"p<,>iiii",gFractionRank[fraction][0], gFractionRank[fraction][1], gFractionRank[fraction][2], gFractionRank[fraction][3]);

		cache_get_value_index_int(r, 17, gFractionFreeze[fraction]);
	}
	printf ("[Ранги организаций] Загружены: %d фракций", rows);
	return 1;
}
publics: OnAntiCheatLoad()
{
	new
		rows = cache_num_rows(),
		tick = GetTickCount();

	if(!rows)
	{
		print("[AntiCheat]: Настройки анти-чита не найдены в базе данных.");
		return 0;
	}

	for(new i = 0; i < AC_MAX_CODE; i++) {
		cache_get_value_name_int(i, "ac_type", AC_CODE_TYPE[i]);

		if(AC_CODE_TYPE[i] == 0 || AC_CODE_TYPE[i] == 3)
			EnableAntiCheat(i, false);
		else
			EnableAntiCheat(i, true);
	}

	for(new i = 0; i < 12; i++) EnableAntiNOP(i, true); // Включаем ВСЕ анти-нопы

	printf("[AntiCheat]: Настройки анти-чита успешно загружены (загружено: %d). Потрачено: %d мс.", rows, GetTickCount() - tick);

	return 1;
}
