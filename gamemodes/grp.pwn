stock CaptureEnd(familyone, familytwo)
{
	new best_frag = 0, /* Лучший игрок на капте by ZeRik (04.03.2018) */
		best_id = 0;

	foreach (new i : Player)
	{
	    if(PlayerInfo[i][pMember] != familyone && PlayerInfo[i][pMember] != familytwo)
				continue;
				
    	if(gBest_Frag[i] > best_frag) {
        	best_frag = gBest_Frag[i];
       		best_id = i;
   		}
   		gBest_Frag[i] = 0;

		for(new td; td < 8; td++)
			TextDrawHideForPlayer(i, tdCapture[td]);
			
		if(!AdminInfo[i][aLogin])
			ClearKillFeed(i);
	}

	if(best_frag > 0) {

		foreach (new i : Player)
		{
			if(PlayerInfo[i][pMember] != familyone && PlayerInfo[i][pMember] != familytwo)
				continue;

			SendMes(i, -1, "Лучший игрок на капте {FF4500}%s (%i) {FFFFFF}который уничтожил {FF4500}%d человек(а)", PlayerInfo[best_id][pNames], best_id, best_frag);
		}

	}
	
	GangInfo[GZInfo[IDzone][gNapad]][captureid] =
	GangInfo[GZInfo[IDzone][gNapad]][capture] =
	GangInfo[GZInfo[IDzone][gNapad]][score] =
	GangInfo[GZInfo[IDzone][gFrak]][captureid] =
	GangInfo[GZInfo[IDzone][gFrak]][capture] =
	GangInfo[GZInfo[IDzone][gFrak]][score] =
	GangInfo[GZInfo[IDzone][gNapad]][gangnumber] =
	GangInfo[GZInfo[IDzone][gFrak]][gangnumber] =
	ZoneOnBattle[IDzone] =
	IsCapture = 0;
}
