stock SetPlayerExtra(playerid, declare, level, const reason[])
{
    if(declare != INVALID_PLAYER_ID)
		SendMes(playerid,COLOR_LIGHTRED,"Вы совершили преступление: [%s]. Сообщил: %s.",reason,PlayerInfo[declare][pNames]);
	else
		SendMes(playerid,COLOR_LIGHTRED,"Вы совершили преступление: [%s]. Сообщил: Неизвестный.",reason);

	if((PlayerInfo[playerid][pWanted] + level) < 6)
		PlayerInfo[playerid][pWanted] += level;
	else
		PlayerInfo[playerid][pWanted] = 6;

	UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `wanted` = '%d' WHERE `id` = '%d'", PlayerInfo[playerid][pWanted], PlayerInfo[playerid][pID]);
	SendMes(playerid,COLOR_YELLOW,"Текущий уровень розыска: %d", PlayerInfo[playerid][pWanted]);
	SetPlayerWantedLevel(playerid, PlayerInfo[playerid][pWanted]);

	if(declare != INVALID_PLAYER_ID)
		format(stringer1, sizeof(stringer1),"Рация: Cообщает: %s. Преступление: %s. Подозреваемый: %s",PlayerInfo[declare][pNames],reason,PlayerInfo[playerid][pNames]);
	else
		format(stringer1, sizeof(stringer1),"Рация: Cообщает: Неизвестный. Преступление: %s. Подозреваемый: %s",reason, PlayerInfo[playerid][pNames]);

	foreach (new i : Player) {
		if(!gPlayerLogged{i})
			continue;

		if(PlayerInfo[i][pMember] == 1 || PlayerInfo[i][pMember] == 2 || PlayerInfo[i][pMember] == 10 || PlayerInfo[i][pMember] == 21 || PlayerInfo[i][pMember] == 25)
			SendClientMessage(i, 0xFEBC41AA, stringer1);
	}

	return 1;
}
