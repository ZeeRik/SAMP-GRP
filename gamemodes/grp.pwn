public OnPlayerEditDynamicObject(playerid, objectid, response, Float:x, Float:y, Float:z, Float:rx, Float:ry, Float:rz)
{
    if(GetPVarInt(playerid, "SelectedObject") == objectid) {
		MoveDynamicObject(objectid, x, y, z, 10.0, rx, ry, rz);
	}
	
	new
	    slot = GetPVarInt(playerid, "playerSelectFurn"),
		house = PlayerInfo[playerid][pPhousekey];

    if(objectid == FurnitureInfo[house][slot][furnObject])
	{
        if(response == EDIT_RESPONSE_FINAL)
		{
            SetDynamicObjectPos(objectid, x, y, z);
			SetDynamicObjectRot(objectid, rx, ry, rz);
			
			FurnitureInfo[house][slot][furnX] = x;
			FurnitureInfo[house][slot][furnY] = y;
			FurnitureInfo[house][slot][furnZ] = z;
			FurnitureInfo[house][slot][furnRX] = rx;
			FurnitureInfo[house][slot][furnRY] = ry;
			FurnitureInfo[house][slot][furnRZ] = rz;
			
           	format(mysql_query_, sizeof(mysql_query_), "UPDATE furniture SET fX = %f, fY = %f, fZ = %f, frX = %f, frY = %f, frZ = %f WHERE hID = %d AND fID = %d",
   				x, y, z, rx, ry, rz, house, slot);
            mysql_tquery(connects, mysql_query_, "", "");
            
            DeletePVar(playerid, "playerSelectFurn");
            SendClientMessage(playerid, -1, "Вы передвинули мебель.");
        }
    }
	else if(objectid == br_player[playerid][br_object_text][0] && br_player[playerid][br_object_text][0] != INVALID_OBJECT)
	{
		if(response == EDIT_RESPONSE_FINAL)
		{
			MoveDynamicObject ( br_player[playerid][br_object_text][0] , x , y , z , 15.0, 0.0 , 0.0 , rz ) ;
			if(blocktext[playerid] == Text3D:-1)
			{
				format(stringer1, sizeof(stringer1), "Табличку установил: {009b69}%s [%d]", PlayerInfo[playerid][pNames], playerid);
				blocktext[playerid] = Create3DTextLabel(stringer1, -1, x, y, z + 1.5, 3.0, GetPlayerVirtualWorld(playerid));
			}

			if(br_player[playerid][br_object_text][1] == INVALID_OBJECT)
			{
				br_player[playerid][br_object_text][1] = CreateDynamicObject(19477,x - 0.05 * floatsin(-rz, degrees), y - 0.05 * floatcos(-rz, degrees), z + 2.6, 0.000, 0.000, rz - 90.0);
				SetDynamicObjectMaterialText(br_player[playerid][br_object_text][1], 0, br_player[playerid][br_text],  120, "Ariel", 33, 1, -8943463, 0 , 1 );
				br_player[playerid][br_text][0] = EOS;
			}
			Streamer_Update(playerid);
		}
		else if(response == EDIT_RESPONSE_CANCEL)
		{
			if(blocktext[playerid] != Text3D:-1)
			{
				Delete3DTextLabel(blocktext[playerid]);
				blocktext[playerid] = Text3D:-1;
			}
			if(br_player[playerid][br_object_text][0] != INVALID_OBJECT)
			{
				DestroyDynamicObject(br_player[playerid][br_object_text][0]);
				br_player[playerid][br_object_text][0] = INVALID_OBJECT;
			}
			br_player[playerid][br_text][0] = EOS;
		}
	}
	return 1;
}
