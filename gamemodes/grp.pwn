		SendMes(params[0], COLOR_YELLOW2, "Военный билет: %s", PlayerInfo[playerid][pVoenBill] ? ("{33AA33}Служил") : ("{AA3333}Не служил"));
		if(PlayerInfo[playerid][pWanted] > 0) SendClientMessage(params[0], COLOR_REDD, "Внимание! Это человек в розыске!");
	}
	else
	{
		if(PlayerInfo[playerid][pFacePass] == 1) SendClientMessage(params[0], COLOR_YELLOW2, "Фракция: Гражданин");
		else if(PlayerInfo[playerid][pFacePass] == 2) SendClientMessage(params[0], COLOR_YELLOW2, "Фракция: FBI");
