stock AddTune(playerid, carnumber)
{
    SetVehicleNumberPlate(caridhouse[playerid], PlayerInfo[playerid][pNumbercar]);
	if(cTune[playerid][carnumber][0] != -1) ChangeVehiclePaintjob(caridhouse[playerid], cTune[playerid][carnumber][0]);
	for(new i = 1; i <= 14; i++) if(cTune[playerid][carnumber][i] != 0) AddVehicleComponent(caridhouse[playerid], cTune[playerid][carnumber][i]);

	if(cTune[playerid][carnumber][15]) {

        new mesto = AttachNeonVehicle(GetVehicleModel(caridhouse[playerid]));
		if(mesto == -1) return true;

	    if(VehicleNeonLeft[caridhouse[playerid]] == INVALID_OBJECT_ID)
		{
            VehicleNeonLeft[caridhouse[playerid]] = CreateDynamicObject(cTune[playerid][carnumber][15], VehicleInfo[caridhouse[playerid]][vX], VehicleInfo[caridhouse[playerid]][vY], VehicleInfo[caridhouse[playerid]][vZ], 0, 0, 0);
            AttachDynamicObjectToVehicle(VehicleNeonLeft[caridhouse[playerid]], caridhouse[playerid], NeonPosLeft[mesto][0], NeonPosLeft[mesto][1], NeonPosLeft[mesto][2], NeonPosLeft[mesto][3], NeonPosLeft[mesto][4], NeonPosLeft[mesto][5]);
		}
		if(VehicleNeonRight[caridhouse[playerid]] == INVALID_OBJECT_ID)
		{
            VehicleNeonRight[caridhouse[playerid]] = CreateDynamicObject(cTune[playerid][carnumber][15], VehicleInfo[caridhouse[playerid]][vX], VehicleInfo[caridhouse[playerid]][vY], VehicleInfo[caridhouse[playerid]][vZ], 0, 0, 0);
            AttachDynamicObjectToVehicle(VehicleNeonRight[caridhouse[playerid]], caridhouse[playerid], NeonPosRight[mesto][0], NeonPosRight[mesto][1], NeonPosRight[mesto][2], NeonPosRight[mesto][3], NeonPosRight[mesto][4], NeonPosRight[mesto][5]);
		}
	}
	return true;
}
