CMD:unit(playerid)
{
	if(!PlayerInfo[playerid][pMember])
		return 1;

	if(!IsACop(playerid) && PlayerInfo[playerid][pMember] != 4
		&& PlayerInfo[playerid][pMember] != 11 && PlayerInfo[playerid][pMember] != 16
		&& PlayerInfo[playerid][pMember] != 7 && !IsAArm(playerid)
		&& PlayerInfo[playerid][pMember] != 23)
			return SendClientMessage(playerid, COLOR_GRAD2, !"Вам недоступно создание подразделения!");

	if(PlayerInfo[playerid][pRank] < gFractionRank[PlayerInfo[playerid][pMember]-1][0])
		return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PlayerInfo[playerid][pMember]-1][0]);

	stringer = "{FFFFFF}Номер\t{FFFFFF}Подразделение\t{FFFFFF}Лидер\n";
	for(new i; i < MAX_UNITS; i++) {
		if(Unit_ID[PlayerInfo[playerid][pMember]-1][i])
			format(stringer, sizeof(stringer),"%s%i.\t{FF6F00}%s{FFFFFF}\t%s\n", stringer, i+1, Unit_Name[PlayerInfo[playerid][pMember]-1][i],(strlen(Unit_Leader[PlayerInfo[playerid][pMember]-1][i])) ? Unit_Leader[PlayerInfo[playerid][pMember]-1][i] : "-");
		else
			format(stringer, sizeof(stringer),"%s%i.\t{FF6F00}-{FFFFFF}\t-\n",stringer, i+1);
	}
	
	return ShowPlayerDialog(playerid, 3854, DIALOG_STYLE_TABLIST_HEADERS, "{FF6F00}Подразделения", stringer, "Выбрать", "Закрыть");
}
CMD:uinvite(playerid,params[])
{
