		return SendClientMessage(playerid, COLOR_WHITE, "Введите: /agivevip [id] [уровень VIP] [кол-во дней]");

	else if(!IsPlayerConnected(params[0]))
		return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует");

	else if(params[1] < 1 || params[1] > 3)
		return SendClientMessage(playerid, COLOR_WHITE, "Уровень VIP не может быть меньше 1 и больше 3");

	switch(params[1]) {
		case 1: SendClientMessage(params[0], COLOR_WHITE, "Поздравляем, вы получили от администрации {FFA500}Bronze VIP");
		case 2: SendClientMessage(params[0], COLOR_WHITE, "Поздравляем, вы получили от администрации {FFA500}Silver VIP");
		case 3: SendClientMessage(params[0], COLOR_WHITE, "Поздравляем, вы получили от администрации {FFA500}Gold VIP");
		default: SendClientMessage(params[0], COLOR_WHITE, "Администратор забрал у вас VIP Аккаунт!");
	}

	if(params[2] == 0)
	{
		PlayerInfo[params[0]][pDonateRank] = PlayerInfo[params[0]][pVipTime] = PlayerInfo[params[0]][pVipData] = 0;

		SendClientMessage(playerid, COLOR_WHITE, "- Вы забрали у игрока VIP Статус.");
		format(stringer1, sizeof(stringer1), "< ALog: [%d LVL] > %s (%i) аннулировал игроку %s (%i) VIP статус", AdminInfo[playerid][aLevel], PlayerInfo[playerid][pNames], playerid, PlayerInfo[params[0]][pNames], params[0]);
	}
	else
	{
	    new year, month,day;
		getdate(year, month, day);
		new curdate = getdate(year,month,day);
		PlayerInfo[params[0]][pDonateRank] = params[1];
		PlayerInfo[params[0]][pVipTime] = curdate+params[2];
		PlayerInfo[params[0]][pVipData] = curdate;

		SendClientMessage(playerid, COLOR_WHITE, "- Вы выдали игроку VIP Статус.");
		format(stringer1, sizeof(stringer1), "< ALog: [%d LVL] > %s (%i) выдал игроку %s (%i) VIP статус %d уровня (%d дней)", AdminInfo[playerid][aLevel], PlayerInfo[playerid][pNames], playerid, PlayerInfo[params[0]][pNames], params[0], params[1], params[2]);
	}
	UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `vip_time` = '%d', `vip_date` = '%d', `donate_rank` = '%d' WHERE `id` = '%d'", PlayerInfo[params[0]][pVipTime], PlayerInfo[params[0]][pVipData], PlayerInfo[params[0]][pDonateRank], PlayerInfo[params[0]][pID]);
	return SendAdminMessage(0xFF0000FF, stringer1);
}
