		return SendClientMessage(playerid, COLOR_GREY, !"Игрок не авторизирован на сервере!");

	else if(AFK_Time[params[0]] > 2)
		return SendClientMessage(playerid, COLOR_GREY, !"Игрок находится в AFK");

	new vehid = GetPlayerVehicleID(targetid),
