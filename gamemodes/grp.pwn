stock GzCheck()
{
	if(GZSafeTime[IDzone] > 0)
	{
		GZSafeTime[IDzone] --;
		TextDrawSetString(tdCapture[2], Convert(GZSafeTime[IDzone]));
	}
	
	if(IsCapture == 2)
	{
        GangInfo[GZInfo[IDzone][gNapad]][score] = GangInfo[GZInfo[IDzone][gFrak]][score] = 0;
	    foreach (new i : Player)
		{
		    if(!gPlayerLogged{i})
				continue;
				
		    if(PlayerToKvadrat(i, GZInfo[IDzone][gCoords][0], GZInfo[IDzone][gCoords][1], GZInfo[IDzone][gCoords][2], GZInfo[IDzone][gCoords][3]))
		    {
		    	if(PlayerInfo[i][pMember] == GZInfo[IDzone][gFrak])
      				GangInfo[GZInfo[IDzone][gFrak]][score] ++;
				else if(PlayerInfo[i][pMember] == GZInfo[IDzone][gNapad])
   					GangInfo[GZInfo[IDzone][gNapad]][score] ++;
			}
	    }
	    
		format(stringer1, sizeof stringer1, "%d", GangInfo[GZInfo[IDzone][gNapad]][score]);
		TextDrawSetString(tdCapture[5], stringer1);

		format(stringer1, sizeof stringer1, "%d", GangInfo[GZInfo[IDzone][gFrak]][score]);
		TextDrawSetString(tdCapture[7], stringer1);
	}
	
	if(ZoneOnBattle[IDzone] == 1 && GZSafeTime[IDzone] == 0)
	{
		if(IsCapture == 1 && (GangInfo[GZInfo[IDzone][gNapad]][score] > GangInfo[GZInfo[IDzone][gFrak]][score] ||
			GangInfo[GZInfo[IDzone][gNapad]][score] == 0 && GangInfo[GZInfo[IDzone][gFrak]][score] == 0)
			|| IsCapture == 2 && (GangInfo[GZInfo[IDzone][gNapad]][score] > 0 && GangInfo[GZInfo[IDzone][gFrak]][score] == 0))
		{
			if(GZInfo[IDzone][gFrak] == 18) TotalGzR--;
			else if(GZInfo[IDzone][gFrak] == 17) TotalGzA--;
			else if(GZInfo[IDzone][gFrak] == 15) TotalGzG--;
			else if(GZInfo[IDzone][gFrak] == 13) TotalGzV--;
			else if(GZInfo[IDzone][gFrak] == 12) TotalGzB--;
			
			if(GZInfo[IDzone][gNapad] == 18) TotalGzR++;
			else if(GZInfo[IDzone][gNapad] == 17) TotalGzA++;
			else if(GZInfo[IDzone][gNapad] == 15) TotalGzG++;
			else if(GZInfo[IDzone][gNapad] == 13) TotalGzV++;
			else if(GZInfo[IDzone][gNapad] == 12) TotalGzB++;
			
			format(stringer1, sizeof stringer1,"[F] Банда %s выйгрывает захват зоны", GetPlayerFracName[GZInfo[IDzone][gNapad]]);
			SendFamilyMessage(GZInfo[IDzone][gFrak], 0x4682B4AA, stringer1);
			SendFamilyMessage(GZInfo[IDzone][gNapad], 0x4682B4AA, stringer1);
			
			if(IDzone == 47)
			{
				format(stringer1, sizeof(stringer1), "Притон\nПод контролем:\n%s\nЦена наркотиков: %d", GetPlayerFracName[GZInfo[IDzone][gNapad]], pricelist);
    			UpdateDynamic3DTextLabelText(PritonLable, COLOR_GREEN, stringer1);
    		}
    		
			GangZoneStopFlashForAll(GZInfo[IDzone][id]);
			GangZoneHideForAll(GZInfo[IDzone][id]);
			GangZoneShowForAll(GZInfo[IDzone][id], GetGangZoneColor(GZInfo[IDzone][gNapad]));
    		
    		CaptureEnd(GZInfo[IDzone][gNapad], GZInfo[IDzone][gFrak]);
    		
    		GZInfo[IDzone][gFrak] = GZInfo[IDzone][gNapad];
    		GZInfo[IDzone][gNapad] = 0;

    		skladband();
    		SaveGZ(IDzone);
    		
    		IDzone = -1;
		}
		else if(IsCapture == 1 && (GangInfo[GZInfo[IDzone][gNapad]][score] <= GangInfo[GZInfo[IDzone][gFrak]][score])
			|| IsCapture == 2 && (GangInfo[GZInfo[IDzone][gFrak]][score] > 0 && GangInfo[GZInfo[IDzone][gNapad]][score] == 0
			|| GangInfo[GZInfo[IDzone][gFrak]][score] == 0 && GangInfo[GZInfo[IDzone][gNapad]][score] == 0))
		{
			format(stringer1, 90, "[F] Вы отстояли свою территорию против банды %s", GetPlayerFracName[GZInfo[IDzone][gNapad]]);
			SendFamilyMessage(GZInfo[IDzone][gFrak], 0xff2400FF, stringer1);
			
			format(stringer1, 90, "[F] Ваша банда не смогла забрать территорию у %s", GetPlayerFracName[GZInfo[IDzone][gFrak]]);
			SendFamilyMessage(GZInfo[IDzone][gNapad], 0xff2400FF, stringer1);
			
			GangZoneStopFlashForAll(GZInfo[IDzone][id]);
			GangZoneHideForAll(GZInfo[IDzone][id]);
			GangZoneShowForAll(GZInfo[IDzone][id], GetGangZoneColor(GZInfo[IDzone][gFrak]));
			
			CaptureEnd(GZInfo[IDzone][gNapad], GZInfo[IDzone][gFrak]);
			
			GZInfo[IDzone][gNapad] = 0;
			IDzone = -1;
		}
	}
}
stock IsInAllowed(playerid)
{
	for(new i = 0; i < sizeof(allowedfactions); i++) {
		if(PlayerInfo[playerid][pMember] == allowedfactions[i])
			return 1;
	}
	return 0;
}
