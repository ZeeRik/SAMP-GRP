	if(cTune[playerid][carnumber][15] && caridhouse[playerid] != INVALID_VEHICLE_ID) DellNeon(caridhouse[playerid]);
	for(new i = 1; i <= 15; i++) cTune[playerid][carnumber][i] = 0;

	if(delveh == 1) {
