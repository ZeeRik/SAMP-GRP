publics: DeleteAccount (playerid, d_nick [])
{
    new
	    rows;

    cache_get_row_count(rows);

    if(!rows)
	    return SendMes(playerid, COLOR_GREY, "Аккаунт %s не найден в базе данных.", d_nick);

    new
	    d_id = GetPlayerID(d_nick);

    if(IsPlayerConnected(d_id)) {
        DeleteAcc{d_id} = true;
        SendClientMessage(d_id, COLOR_REDD, !"* Ваш аккаунт был удален за нарушения правил сервера!");
        SendClientMessage(d_id, COLOR_REDD, !"* Пожалуйста, сообщите нам, если Вы несогласны с действиями администратора.");
        KickEx(d_id);
    }

    /* Очищаем бизнес */
    for(new i = 1; i <= TOTALBIZZ; i++) {

	    if(!strcmp(BizzInfo[i][bOwner], d_nick, false)) {

	        ClearBizz(i, 1);
	        break;
	    }
    }
    
    /* Очищаем дом */
    for(new i = 0; i < sizeof(HouseInfo); i++) {

        if(!strcmp(HouseInfo[i][hOwner], d_nick, false)) {

	        ClearHouse(i);
	        break;
	    }
	}
	
	/* Очищаем семью */
	format(mysql_query_, sizeof(mysql_query_), "SELECT id FROM family WHERE leader = '%s'", d_nick);
	new Cache:result = mysql_query(connects, mysql_query_);

	if(cache_num_rows())
	{
		new family;
		cache_get_value_name_int(0, "id", family);

		ClearFamily(family);
	}
	cache_delete(result);

	/* Очищаем билборды */
	for(new x; x < MAX_BILLBORDS; x++) {
	
	    if(!strcmp(BildInfo[x][bOwner], d_nick, false)) {
	    
			ClearBillBord(x);
		}
	}
	
    /* Удаляем аккаунт и все содержимое о нем */
    mysql_format(connects, mysql_query_, sizeof(mysql_query_), "DELETE FROM `"TABLE_ACCOUNT"` WHERE `name` = '%e'", d_nick);
    mysql_tquery(connects, mysql_query_, "", "");

    format(stringer1, sizeof(stringer1), "Главный администратор %s удалил аккаунт %s за нарушения правил сервера!", PlayerInfo[playerid][pNames], d_nick);
    SendAdminMessage(COLOR_REDD, stringer1);

    Admin_Log(playerid, "удалил аккаунт", d_nick);

    return true;
}
publics: PromoCode_Check(playerid, promo[]) /* PROMO by ZeRik (03.03.2018) */
{
	if(!cache_num_rows())
		return SendMes(playerid, -1, "Промокод {4F94CD}%s{FFFFFF} уже использован или несуществует", promo);

	new condition,
		money,
		donate,
		level,
		exp,
		max_use;

    cache_get_value_name_int(0, "hours_in_game", condition);
	cache_get_value_name_int(0, "money", money);
	cache_get_value_name_int(0, "donate", donate);
	cache_get_value_name_int(0, "level", level);
	cache_get_value_name_int(0, "exp", exp);
	cache_get_value_name_int(0, "max_use", max_use);

	if(PlayerInfo[playerid][pPayDayHad] < condition)
		return SendMes(playerid, -1, "Для активации промокода {4F94CD}%s{FFFFFF} необходимо отыграть не менее {4F94CD}%d{FFFFFF} часов", promo, condition);

	if(money != 0) {
		PlayerInfo[playerid][pCash] += money;
		SendMes(playerid, -1, "Получено {4F94CD}%d{FFFFFF} наличными", money);
	}

	if(donate != 0) {
		PlayerInfo[playerid][pDonatemoney2] += donate;
		SendMes(playerid, -1, "Получено {4F94CD}%d{FFFFFF} донат рублей", donate);
	}

	if(level != 0) {
		PlayerInfo[playerid][pLevel] += level;
		SendMes(playerid, -1, "Ваш уровень повышен до {4F94CD}%d", PlayerInfo[playerid][pLevel]);
		UpdatePlayerData("UPDATE `"TABLE_ACCOUNT"` SET `lvl` = '%d' WHERE `id` = '%d'", PlayerInfo[playerid][pLevel], PlayerInfo[playerid][pID]);
	}

	if(exp != 0) {
		PlayerInfo[playerid][pExp] += exp;
		SendMes(playerid, -1, "Получено {4F94CD}%d{FFFFFF} опыта", exp);
	}

	if(-- max_use <= 0) {
	    mysql_format(connects, mysql_query_, sizeof(mysql_query_), "DELETE FROM promo_code WHERE promo = '%e'", promo);
	    mysql_tquery(connects, mysql_query_, "", "" );
	}

	else
	{
	    mysql_format(connects, mysql_query_, sizeof(mysql_query_), "UPDATE promo_code SET max_use = %d WHERE promo = '%e'", max_use, promo);
	    mysql_tquery(connects, mysql_query_, "", "" );

	    mysql_format(connects, mysql_query_, sizeof(mysql_query_), "INSERT INTO promo_use ( `promo`, `account`, `use_date` ) VALUES ( '%e', '%d', %d)", promo, PlayerInfo[playerid][pID], gettime());
        mysql_tquery(connects, mysql_query_, "", "" );
	}
	return true;
}
