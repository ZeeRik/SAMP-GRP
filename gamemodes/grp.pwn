        if(adist <= 0) adist = 1000.0;
        GetPlayerPos(playerid, music[0], music[1], music[2]);
        area = CreateDynamicSphere(music[0],music[1],music[2], adist*2.0, -1, -1, -1);
        SendClientMessage(playerid,COLOR_GREY,"Вы включили радио и назначили дистанцию.");
        turned = true;
        foreach(Player, i) if(IsPlayerInDynamicArea(i,area)) PlayAudioStreamForPlayer(i, aurl, music[0], music[1], music[2], adist, true);
