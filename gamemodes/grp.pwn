stock SetWbook(playerid, status = 0)
{
    if(IsCapture > 0 && IsAGang(playerid)
		&& (PlayerInfo[playerid][pMember] == GZInfo[IDzone][gNapad] || PlayerInfo[playerid][pMember] == GZInfo[IDzone][gFrak]))
	{
	    if(!status)
	    {
			for(new td; td < 8; td++)
				TextDrawShowForPlayer(playerid, tdCapture[td]);

        	GangZoneFlashForPlayer(playerid, GZInfo[IDzone][id], GetGangZoneColor(GZInfo[IDzone][gNapad]));
		}
		else
		{
			gBest_Frag[playerid] = 0;

			for(new td; td < 8; td++)
				TextDrawHideForPlayer(playerid, tdCapture[td]);

			if(!AdminInfo[playerid][aLogin])
				ClearKillFeed(playerid);
		}
	}
	
	if(AdminInfo[playerid][aLevel])
	    return 1;
	
	if(!status)
	{
		format(mysql_query_, sizeof(mysql_query_), "INSERT INTO `employment_history`(`account`, `date_invite`) VALUES ('%d', NOW())", PlayerInfo[playerid][pID]);
		return mysql_tquery(connects, mysql_query_, "", "");
	}

    if(PlayerInfo[playerid][pRank] >= 3)
    {
	    mysql_format(connects, mysql_query_, sizeof(mysql_query_), "UPDATE `employment_history` SET `date_uninvite` = NOW(), `rang` = '%d', `frac_name` = '%e', `position` = '%e' WHERE `account` = '%d'",
	    PlayerInfo[playerid][pRank], GetPlayerFracName[PlayerInfo[playerid][pMember]], fRankName[PlayerInfo[playerid][pMember]-1][PlayerInfo[playerid][pRank]-1], PlayerInfo[playerid][pID]);
	}
	else
	{
		format(mysql_query_,sizeof(mysql_query_),"DELETE FROM `employment_history` WHERE `account` = '%d' AND `date_uninvite` IS NULL AND `rang` IS NULL AND `frac_name` = 'unknown_frac' AND `position` = 'unknown_position'", PlayerInfo[playerid][pID]);
	}
	
	return mysql_tquery(connects, mysql_query_, "", "");
}
stock gc_GetFreeSeat(vehicleid)
{
    new
		model = GetVehicleModel(vehicleid),
        tmp = 0,
        seat = 0;

	if(!model)
		return -1;

    foreach(new i : Player)
    {
        if(AntiCheatGetVehicleID(i) == vehicleid)
        {
            if(0 <= (seat = GetPlayerVehicleSeat(i)) < cellbits)
            {
                tmp |= 1 << seat;
            }
        }
    }

    tmp = ~tmp;
    for(new i = 1; i < gc_vehicle_max_seats[model - 400];
    	i++)
    {
    	if((~(tmp - 1) & tmp) == 1 << i)
    	{
    		return i;
    	}
    }
    return -1;
}
