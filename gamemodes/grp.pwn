public OnPlayerDamage(&playerid, &Float:amount, &issuerid, &weapon, &bodypart)
{
	if(playerid == INVALID_PLAYER_ID || issuerid == INVALID_PLAYER_ID)
		return 1;

    else if(PlayerZombie{issuerid} == 2 && PlayerZombie{playerid} == 0) {
		PlayerZombie{playerid} = 1;
		SendClientMessage(playerid, -1, !"{DC143C}Кажется, вы почувствовали недомагания. Проверьтесь у доктора");
    }
	else if(StartPaintBall == 2 && TeamPain[playerid] != 0 && TeamPain[issuerid] != 0 && TeamPain[playerid] == TeamPain[issuerid])
		return 0;

	else if(AdminInfo[playerid][aLogin] && AdminInfo[playerid][aGm])
		return 0;

    else if((IsACop(issuerid) && Tazer[issuerid] == 0 && !PlayerCuffed[playerid] && !IsPlayerInAnyVehicle(issuerid))
		&& (GetPVarInt(issuerid , "tazershoot") > 0 && weapon == GetPVarInt ( issuerid , "tazergun" )) || weapon == 3)
	{
		TogglePlayerControllable(playerid, 0);
		PlayerCuffed[playerid] = 1; PlayerCuffedTime[playerid] = 15;  Tazer[issuerid] = 1; TazerTime[issuerid] = 15;

		if(weapon == 3)
			FIXES_ApplyAnimation(playerid,"PED","KO_skid_front",4.1,0,1,1,1,0);
		else
		{
			if(GetPVarInt (issuerid , "tazershoot") > 0) SetPVarInt (issuerid , "tazershoot", GetPVarInt (issuerid,"tazershoot" ) - 1);

			if (GetPVarInt (issuerid, "tazershoot" ) == 0) {
				SendClientMessage (issuerid, COLOR_ISPOLZUY, !"[ ! ] {FFFFFF}Парализующие патроны выстреляны");
				DeletePVar(issuerid , "tazershoot");
				DeletePVar(issuerid , "tazergun");
			}

			FIXES_ApplyAnimation(playerid, "PED", "FLOOR_hit_f", 4.0, 0, 1, 1, 1, 0, 1);
		}

		format(stringer1, sizeof(stringer1), "Полицейский %s оглушил гражданина %s %s", PlayerInfo[issuerid][pNames], PlayerInfo[playerid][pNames], (weapon == 3) ? "дубинкой" : "электрошокером");
		ProxDetector(playerid, 30.0, COLOR_ISPOLZUY, stringer1);

		SendClientMessage(issuerid, COLOR_WHITE, !"(( Нажмите {FF0000}\"N\"{FFFFFF} около оглушенного игрока, чтобы надеть на него наручники ))");

		SetPVarInt ( playerid , "TazerPlayer", issuerid ) ;
		SetPVarInt ( issuerid , "TazerPlayer", playerid + 1) ;
		return 0;
	}

	else if(IsAGreenZone(playerid) || IsAGreenZone(issuerid)) {
		FIXES_ApplyAnimation(issuerid, "FAT", "IDLE_tired",4.0,1,0,0,0,0,1);
	    SetPVarInt(issuerid,"AntiDM",8);
	    return 0;
	}

	new
		avg_rate = AverageShootRate(issuerid, 2); /*CBug Fixed*/

    if (avg_rate != -1) {
        if (weapon == WEAPON_DEAGLE && avg_rate < 500)
            amount *= 0.50;
        else if (weapon == WEAPON_SHOTGSPA && avg_rate < 250)
            amount *= 0.50;
    }

	return 1;
}
stock GiveMoney(playerid, amount)
{
	if(PlayerInfo[playerid][pCash] + amount < PlayerInfo[playerid][pCash]) PlayerInfo[playerid][pCash] = 2100000000;
	else PlayerInfo[playerid][pCash] += amount, GivePlayerMoney(playerid, amount);
	return true;
}
stock GiveBank(playerid, amount)
{
	if(PlayerInfo[playerid][pBank] + amount < PlayerInfo[playerid][pBank]) PlayerInfo[playerid][pBank] = 2100000000;
	else PlayerInfo[playerid][pBank] += amount;
	return true;
}
