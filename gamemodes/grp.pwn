publics: OnPBotUpdate()
{
	if(gTimerDelay)
		gTimerDelay--;

	else if(gSetting) {

		new
			hour;

		gettime(hour);
		if(gAtHour[hour] < gFakeCount && gFakeCount)
		{
			foreach(new botid : Player) {
				if(playerPBot[botid] == -1)
					continue;

				Kick(botid);
				break;
			}
		}
		if(gAtHour[hour] > gFakeCount && gFakeCount < gSlotCount && Iter_Count(Player) < gSlotCount)
		{
			NameCreate:
			format(stringer1, sizeof(stringer1), "%s_%s", Firstname[random(171)], Lastname[random(213)]);

			foreach(new botid : Player) {
				if(playerPBot[botid] == -1)
					continue;

				if(!strcmp(PlayerInfo[botid][pNames], stringer1)) {
					goto NameCreate;
				}
			}

			gFakeCount ++;
			PB_RegisterBot(stringer1);
			ConnectNPC(stringer1, "pawnbots");
		}

		if(++ gTimerDCount >= gTimerDLRand)
	    {
	        gTimerDelay = (5 + random(6));
	        gTimerDLRand = (2 + random(3));
	        gTimerDCount = 0;
		}
	}

	return 1;
}
