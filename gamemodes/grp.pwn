CMD:mute(playerid, params[])
{
	if((!AdminInfo[playerid][aLevel] || !AdminInfo[playerid][aLogin]) && PlayerInfo[playerid][pHelper] < 3)
		return 1;

	extract params -> new player:targetid, time = 2, string:reason[65] = "причина не указана"; else
        return SendClientMessage(playerid, -1, !"Введите: /mute [игрок] [минуты] [причина]");

	if(!IsPlayerConnected(targetid) || targetid == playerid)
	    return SendClientMessage(playerid, COLOR_GRAD1, !"Ошибка: Игрока не существует или это Вы");

	if(IsIpFound(reason) || CheckString(reason))
		return DelAdmin(playerid, 1);

	if(AdminInfo[targetid][aLevel] > AdminInfo[playerid][aLevel])
	    return SendClientMessage(playerid, COLOR_GRAD2, !"Ошибка: игрок не может быть наказан");
	
	if(!AdminInfo[targetid][aLevel] && !AdminInfo[playerid][aLevel] &&
		(PlayerInfo[targetid][pHelper] > PlayerInfo[playerid][pHelper]))
		return SendClientMessage(playerid, COLOR_GRAD2, !"Ошибка: игрок не может быть наказан");

	if (time < 1 || time > 60)
		return SendClientMessage(playerid, COLOR_GRAD1, "Ошибка: время должно быть от 1 - 60");

	if (PlayerInfo[targetid][pMuted] > 0)
		return SendClientMessage(playerid, COLOR_GRAD1, "Ошибка: игрок уже заткнут");

    PlayerInfo[targetid][pMuted] = (time * 60);
	PlayerPlaySound(targetid, 1057, 0.0, 0.0, 0.0);
	SendMes(targetid, -1, "Вы получили бан чата на %i минут", time);

	format(stringer1, sizeof(stringer1), "%s {FF0000}%s {FFFFFF}заткнул {FF0000}%s{FFFFFF} на {FF0000}%i минут(ы){FFFFFF}. Причина: {FF0000}%s",
		AdminInfo[playerid][aLevel] ? ("Админ") : ("Хелпер"), PlayerInfo[playerid][pNames], PlayerInfo[targetid][pNames], time, reason);

	return SendAdminMessageToAll(-1, stringer1, targetid);
}
