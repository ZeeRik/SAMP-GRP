//--------------------------------------------------------[ При регистрации  закрытие] ----------------------------------------------------------
public OnPlayerStateChange(playerid, newstate, oldstate)
{
	if (!gIsRequestClassCalled{playerid}
        && newstate == PLAYER_STATE_ONFOOT
        && oldstate == PLAYER_STATE_NONE) {
        return Kick(playerid);
    }

	new
	    newcar = GetPlayerVehicleID(playerid);

    if(isPlayerSpectate[playerid] != 0 && (newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_PASSENGER))
	{
	    foreach (new i : Admin) {
			if (spectateID[i] == playerid) {
    			PlayerSpectateVehicle(i, newcar, SPECTATE_MODE_NORMAL);

				ShowVehicleInfo(i, newcar);
    			spectateType[i] = SPECTATE_TYPE_VEHICLE;
			}
		}
	}
	else if(newstate == PLAYER_STATE_SPECTATING)
	{
	    if(isPlayerSpectate[playerid] != 0)
	    {
	        new
				prev = GetPreviousPlayer(playerid);

	        if (prev == INVALID_PLAYER_ID)
			{
			    foreach (new i : Admin) {
					if (spectateID[i] == playerid) {
					    isPlayerSpectate[playerid] --;
						StopSpectate(i);
					}
				}
			}
			else
			{
			    foreach (new i : Admin) {
					if (spectateID[i] == playerid) {
					    isPlayerSpectate[playerid] --;
					    isPlayerSpectate[prev] ++;
						StartSpectate(i, prev);
					}
				}
			}
		}
		
    	Iter_Remove(SpectatePlayers, playerid);
	}
	
	if(oldstate == PLAYER_STATE_ONFOOT && newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_PASSENGER) {
 		if(radioWave[newcar] != 0) {
 		    radioOnPlayer{playerid} = true;
	 		PlayAudioStreamForPlayer(playerid, radio[radioWave[newcar]][radio_URL]);
		}
	}
    else if(oldstate == PLAYER_STATE_DRIVER || oldstate == PLAYER_STATE_PASSENGER) {
        if(radioOnPlayer{playerid} == true) StopAudioStreamForPlayer(playerid);
	}
	
	if(newstate == PLAYER_STATE_ONFOOT)
	{
	    if(isPlayerSpectate[playerid] != 0) {
	        foreach (new i : Admin) {
				if (spectateID[i] == playerid) {
	    			PlayerSpectatePlayer(i, playerid, SPECTATE_MODE_NORMAL);

					if(spectateType[i] == SPECTATE_TYPE_VEHICLE) HideVehicleInfo(i);
	    			spectateType[i] = SPECTATE_TYPE_PLAYER;
				}
			}
		}
		
	    if(!IsAVel(newcar))
		{
            for( new text; text != 5; text++) TextDrawHideForPlayer(playerid, textVelocimetro[text]);
            for( new text; text != 4; text++) PlayerTextDrawHide(playerid, textPlayerVelocimetro[text][playerid]);
		}
		if(TransportDuty[playerid] > 0) SetVehicleToRespawn(GetPlayerVehicleID(playerid));
		if(PlayerInfo[playerid][pJob] == 2)
		{
			if(gcontract[playerid] == 1) Delete3DTextLabel(Meh3d[GetPlayerVehicleID(playerid)]),gcontract[playerid] = 0;
		}
		if(TalkingLive[playerid] == 2)
		{
			SendClientMessage(playerid, 0x6ab1ffaa, "Прямой эфир завершён");
			SendClientMessage(TalkingLive[playerid], 0x6ab1ffaa, "Прямой эфир завершён");
			TogglePlayerControllable(playerid, true);
			TogglePlayerControllable(TalkingLive[playerid], 1);
			TalkingLive[TalkingLive[playerid]] = INVALID_PLAYER_ID;
			TalkingLive[playerid] = INVALID_PLAYER_ID;
		}
		else if(TalkingLivels[playerid] == 2)
		{
			SendClientMessage(playerid, 0x6ab1ffaa, "Прямой эфир завершён");
			SendClientMessage(TalkingLivels[playerid], 0x6ab1ffaa, "Прямой эфир завершён");
			TogglePlayerControllable(playerid, true);
			TogglePlayerControllable(TalkingLivels[playerid], 1);
			TalkingLivels[TalkingLivels[playerid]] = INVALID_PLAYER_ID;
			TalkingLivels[playerid] = INVALID_PLAYER_ID;
		}
		if(TakingLesson[playerid] == 1)
		{
			SendClientMessage(playerid, COLOR_RED, "Автосдача прекращена, вы покинули автомобиль");
