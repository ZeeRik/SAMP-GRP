CMD:invite(playerid, params[])
{
	if(!PlayerInfo[playerid][pMember])
		return SendClientMessage(playerid, COLOR_GREY, "У вас нет прав доступа к этой команде!");

	if(PlayerInfo[playerid][pRank] < gFractionRank[PlayerInfo[playerid][pMember]-1][0])
		return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PlayerInfo[playerid][pMember]-1][0]);

	switch(PlayerInfo[playerid][pMember]) {
		case 1..4, 7, 10, 11, 19, 21, 25, 26: if(PlayerInfo[playerid][pOdet] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!");
	}

	extract params -> new player:targetid, rang = 1; else
        return SendClientMessage(playerid, -1, !"Введите: /invite [игрок] [ранг (опц)]");

	if(!IsPlayerConnected(targetid) || !PlayerInfo[targetid][pID])
		return SendClientMessage(playerid, COLOR_GREY, !"Игрок не авторизирован на сервере!");

    if(PlayerInfo[targetid][pWarns])
		return SendClientMessage(playerid, COLOR_GREY, !"У игрока Warn.");

	if(IsPlayerInAnyVehicle(targetid))
		return SendClientMessage(playerid, COLOR_GREY, !"Игрок в машине.");

	if(PlayerInfo[targetid][pMember] > 0 || PlayerInfo[targetid][pLeader] > 0)
		return SendClientMessage(playerid, COLOR_GREY, !"Игрок уже где то состоит");

	if(!ProxDetectorS(10.0, playerid, targetid) || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(targetid))
		return SendClientMessage(playerid, COLOR_GREY, !"Вы далеко друг от друга");

	switch(PlayerInfo[playerid][pMember])
	{
		case 1,10,3,19,21: if(rang > 14 || rang < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 14!");
        case 2,26,4,5,6,11,12,13,14,15,17,18,22,24,25,7: if(rang > 9 || rang < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 9!");
        case 27,28: if(rang > 4 || rang < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 4!");
        case 16: if(rang > 12 || rang < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 12!");
        case 23: if(rang > 11 || rang < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 11!");
	}
	InvitePlayer[targetid] = playerid;
	InviteFrac[targetid] = PlayerInfo[playerid][pMember];
	InviteRank[targetid] = rang;

	SendMes(playerid, -1, "Вы предложили {FF6F00}%s{FFFFFF} вступить в организацию {FF6F00}%s {FFFFFF}на {FF6F00}%d {FFFFFF}ранг.", PlayerInfo[targetid][pNames], GetPlayerFracName[PlayerInfo[playerid][pMember]], rang);

	format(stringer, sizeof(stringer),"{FFFFFF}%s предлагает Вам вступить в организацию: {FF6F00}%s на %d ранг{FFFFFF}\n\nНажмите \"Да\" для согласия или \"Нет\" для отказа!",PlayerInfo[playerid][pNames],GetPlayerFracName[PlayerInfo[playerid][pMember]], rang);
	return ShowPlayerDialog(targetid, 76, DIALOG_STYLE_MSGBOX, "{FF6F00}Вступление во фракцию", stringer, "Да", "Нет");
}
